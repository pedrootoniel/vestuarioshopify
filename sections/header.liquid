              {%- if section.settings.icon_type_style != 'default-text' -%}
                <div class="wishlist-count-bubble l-h-1 pos-absolute b-circle d-flex j-c-center a-i-center">
                  <span class="number" aria-hidden="true">0</span>
                  <span class="visually-hidden" data-text="{{ 'templates.wishlist.items' | t }}">0 {{ 'templates.wishlist.items' | t }}</span>
                </div>
              {%- else -%}
                <div class="wishlist-text-count wishlist-count-bubble d-flex function-menu-item">  
                  (<div class="wishlist-icon-bubble">
                    <span class="number" aria-hidden="true">0</span>
                    <span class="visually-hidden" data-text="{{ 'templates.wishlist.items' | t }}">0 {{ 'templates.wishlist.items' | t }}</span>
                  </div>)
                </div>
              {%- endif -%}
            </button>
            <noscript>
              <a href="{{ routes.cart_url }}" class="link u-none color-foreground focus-inset {{ section.settings.font_transform }}">
                {{ icon_wishlist }}
                <span class="visually-hidden">{{ 'templates.wishlist.wishlist' | t }}</span>
              </a>
            </noscript>
          </side-drawer-opener>
        {%- endif -%}
        {%- if section.settings.enable_cart or section.settings.enable_cart_mobile -%}
          {%- if settings.cart_type == 'drawer' -%}
            <side-drawer-opener 
              class="header__icon header__icon--cart d-flex j-c-center a-i-center p-0 pos-relative u-none color-foreground f-a-none
                {% unless section.settings.enable_cart %} d-none-lg{% endunless %}
                {% unless section.settings.enable_cart_mobile %} d-none-mw-md{% endunless %}
              " 
              data-side-drawer="#Cart-Drawer"
            >
              <button class="t-button b-none c-pointer p-0 color-foreground bg-transparent focus-inset no-js-hidden{% if section.settings.icon_type_style == 'default-text' %} u-none link {{ section.settings.link_type | prepend: "link--style-" }} {{ section.settings.text_size | prepend: 'text-size-' }}{% endif %} {{ section.settings.font_transform }}{% if section.settings.icon_type_style == 'default-text' %} d-flex c-gap{% endif %}" 
                type="button" 
                aria-haspopup="dialog" 
                aria-label="{% if section.settings.icon_type_style == 'default-text' %}{{ 'templates.cart.cart_texts' | t: count: cart.item_count }}{% else %}{{ 'templates.cart.cart_items' | t: count: cart.item_count }}{% endif %}"
                {%- if section.settings.icon_type_style == 'default-text' %}
                  style="--c-gap: 0.3rem"
                {% endif -%}
              >
                {{ icon_cart }}
                {%- if cart.item_count < 100 -%}
                  {%- if section.settings.icon_type_style != 'default-text' -%}
                    <div class="cart-count-bubble l-h-1 pos-absolute b-circle d-flex j-c-center a-i-center cart-icon-bubble">
                      <span aria-hidden="true">{{ cart.item_count }}</span>
                      <span class="visually-hidden">{{ 'sections.header.cart_count' | t: count: cart.item_count }}</span>
                    </div>
                  {%- else -%}
                    <div class="cart-text-count d-flex function-menu-item">  
                      (<div class="cart-icon-bubble">
                        <span aria-hidden="true">{{ cart.item_count }}</span>
                        <span class="visually-hidden">{{ 'sections.header.cart_count' | t: count: cart.item_count }}</span>
                      </div>)
                    </div>
                  {%- endif -%}
                {%- endif -%}
              </button>
              <noscript>
                <a href="{{ routes.cart_url }}" class="link u-none color-foreground focus-inset {{ section.settings.font_transform }}{% if section.settings.icon_type_style == 'default-text' %} d-flex c-gap{% endif %}"
