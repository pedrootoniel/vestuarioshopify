                  widths: widths,
                  height: logo_height,
                  width: section.settings.logo_width,
                  alt: logo_alt,
                  sizes: sizes,
                  preload: true
                }}
              {%- endif -%}
              {%- if has_transparent -%}
                {%- assign logo_transparent_alt = section.settings.logo_transparent.alt | default: shop.name | escape -%}
                {%- assign logo_transparent_height = section.settings.logo_width | divided_by: section.settings.logo_transparent.aspect_ratio -%}
                {% capture sizes %}(max-width: {{ section.settings.logo_width | times: 2 }}px) 100vw, {{ section.settings.logo_width }}px{% endcapture %}
                {% capture widths %}{{ section.settings.logo_width }}, {{ section.settings.logo_width | times: 1.5 | round }}, {{ section.settings.logo_width | times: 2 }}{% endcapture %}
