  "settings": [
  {
    "type": "select",
    "id": "type_style",
    "options": [
      {
        "value": "special",
        "label": "t:settings_schema.layout.settings.style.options__1"
      },
      {
        "value": "classic",
        "label": "t:settings_schema.layout.settings.style.options__2"
      }
