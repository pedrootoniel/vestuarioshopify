<section 
  class="section__m-product spacing spacing-xs color-{{ section.settings.color_scheme }} gradient pos-relative"
  style="--padding-top: {{ section.settings.padding_top }}px;
  --padding-bottom: {{ section.settings.padding_bottom }}px;
  --padding-top-xs: {{ section.settings.padding_top_mobile }}px;
  --padding-bottom-xs: {{ section.settings.padding_bottom_mobile }}px;"
>
  {%- liquid
    assign product = section.settings.product
    assign first_3d_model = product.media | where: 'media_type', 'model' | first
    capture icon_arrow_list
      render 'icon-arrow-list'
    endcapture
  -%}
  {{ 'section-main-product.css' | asset_url | stylesheet_tag }}
  {%- if first_3d_model -%}
    <link
      id="ModelViewerStyle"
      rel="stylesheet"
      href="https://cdn.shopify.com/shopifycloud/model-viewer-ui/assets/v1.0/model-viewer-ui.css"
      media="print"
      onload="this.media='all'">
    <link
      id="ModelViewerOverride"
      rel="stylesheet"
      href="{{ 'component-model-viewer-ui.css' | asset_url }}"
      media="print"
      onload="this.media='all'">
  {%- endif -%}
  <div class="page-width m-auto">
    <div class="featured-product product product--{{ section.settings.media_size }} product--{{ section.settings.gallery_layout }} product--style-{{ section.settings.type_style }} d-flex f-wrap{% if product.media.size == 0 %} product--no-media m-auto{% endif %} r-gap" style="--r-gap: 3rem;">
      <div class="product__media-wrapper w-100 pos-relative f-grow f-shrink">
        {%- if section.settings.product != blank -%}
          {% render 'product-media-gallery',
            product: product,
            gallery_layout: section.settings.gallery_layout,
            media_size: section.settings.media_size,
            enable_video_looping: section.settings.enable_video_looping,
            type_style: section.settings.type_style,
            show_product_badges: section.settings.show_product_badges,
            carousel_direction: section.settings.carousel_direction,
            carousel_gap_tablet: section.settings.carousel_gap_tablet,
            carousel_gap_desktop: section.settings.carousel_gap_desktop,
            ratio_square: section.settings.image_ratio
          %}
        {%- else -%}
          <media-gallery
            id="MediaGallery-{{ section.id }}"
            role="region"
            aria-label="{{ 'products.product.media.gallery_viewer' | t }}"
            data-desktop-layout="stacked"
          >
            <div
              id="GalleryViewer-{{ section.id }}"
              class="product__media-list{% if settings.animations_reveal_on_scroll %} scroll-trigger animate--fade-in{% endif %}"
            >
              <div class="product__media-item">
                <div
                  class="product-media-container global-media-settings gradient{% if section.settings.constrain_to_viewport %} constrain-height{% endif %}"
                  style="--ratio: 1.0; --preview-ratio: 1.0;"
                >
                  {{ 'lifestyle-1' | placeholder_svg_tag: 'placeholder-svg' }}
                </div>
              </div>
            </div>
          </media-gallery>
        {%- endif -%}
        {%- if section.settings.show_product_badges and section.settings.type_style != 'special' -%}
          {%- liquid
            if settings.badge_position == 'top left'
              assign position_class = 'top-0 left-0'
            else
              assign position_class = 'top-0 right-0'
            endif

            if settings.sale_badge_type == 'discount'
              assign current_variant = product.selected_or_first_available_variant
              if current_variant.compare_at_price > current_variant.price
                assign product_discount_price = current_variant.compare_at_price | minus: current_variant.price | times: 100.0 | divided_by: current_variant.compare_at_price | round | append: '%' | prepend: '-'
              endif
            endif
          -%}
          <div class="product__badge pos-absolute z-index-1 d-flex f-column {{ settings.badge_position }} {{ settings.badge_transform }} {{ position_class }}">
            {%- if product.compare_at_price > product.price and product.available -%}
              <span id="Badge-{{ section.id }}-{{ product.id }}" 
              class="badge sale-badge d-inline-block center break {{ settings.badge_button_type }}"
            >
              {%- if settings.sale_badge_type == 'discount' -%}
                {{ product_discount_price }}
              {%- else -%}
                {{- 'products.product.on_sale' | t -}}
              {%- endif -%}
            </span>
            {%- endif -%}
            {%- if product.available == false -%}
              <span id="Badge-{{ section.id }}-{{ product.id }}" 
              class="badge sold-out-badge d-inline-block center break {{ settings.badge_button_type }}"
            >
              {{- 'products.product.sold_out' | t -}}
            </span>
            {%- endif -%}
          </div>
        {%- endif -%}
        {%- if section.settings.enable_share and section.settings.type_style == 'classic' -%}
          <div class="product__share pos-absolute z-index-2">
            {% assign share_url = product.selected_variant.url | default: product.url | prepend: request.origin %}
            {% render 'share-button', section: section, share_link: share_url %}
          </div>
        {%- endif -%}
      </div>
      <div class="product__info-wrapper w-100 f-grow f-shrink product-view-{{ section.settings.type_style }}{% if section.settings.type_style == 'special' %} pos-relative{% endif %}{% if settings.animations_reveal_on_scroll %} scroll-trigger animate--fade-in{% endif %}">
        <product-info
          id="ProductInfo-{{ section.id }}"
          data-section="{{ section.id }}"
          data-url="{{ product.url }}"
          class="product__info-container d-block"
        >
        {%- if section.settings.enable_share -%}
          {%- if section.settings.type_style != 'classic' -%}
            <div class="product__share pos-absolute z-index-2 top-0 right-0">
              {% assign share_url = product.selected_variant.url | default: product.url | prepend: request.origin %}
              {% render 'share-button', section: section, share_link: share_url %}
            </div>
          {%- endif -%}
        {%- endif -%}
          {%- assign product_form_id = 'product-form-' | append: section.id -%}
          {%- for block in section.blocks -%}
            {%- case block.type -%}
              {%- when '@app' -%}
                {% render block %}
              {%- when 'text' -%}
                <p class="product__text element-spacing-all{% if settings.animations_reveal_on_scroll %} scroll-trigger animate--fade-in{% endif %}"
                  {{ block.shopify_attributes }}
                  {% if settings.animations_reveal_on_scroll %}
                    style="--animation-order: {{ forloop.index }};
                      --padding-top: {{ block.settings.padding_top }}px;
                      --padding-bottom: {{ block.settings.padding_bottom }}px;"
                  {% else %}
                    style="--padding-top: {{ block.settings.padding_top }}px;
                      --padding-bottom: {{ block.settings.padding_bottom }}px;"
                  {% endif %}
                >
                  {{ block.settings.text }}
                </p>
              {%- when 'title' -%}
                <div class="product__title break element-spacing-all{% if settings.animations_reveal_on_scroll %} scroll-trigger animate--fade-in{% endif %}" 
                  {{ block.shopify_attributes }}
                  {% if settings.animations_reveal_on_scroll %}
                    style="--animation-order: {{ forloop.index }};
                      --padding-top: {{ block.settings.padding_top }}px;
                      --padding-bottom: {{ block.settings.padding_bottom }}px;"
                  {% else %}
                    style="--padding-top: {{ block.settings.padding_top }}px;
                      --padding-bottom: {{ block.settings.padding_bottom }}px;"
                  {% endif %}
                >
                  <h1 class="{% if block.settings.heading_size == 'h1' or block.settings.heading_size == 'h2' %} {{ block.settings.heading_size }}{%  else %}p-w__heading {{ block.settings.heading_size | prepend: 'heading-size-' }}{% endif %} m-0">
                    {%- if product.title != blank -%}
                      {{ product.title | escape }}
                    {%- else -%}
                      {{ 'onboarding.product_title' | t }}
                    {%- endif -%}
                  </h1>
                </div>
              {%- when 'vendor' -%}
                {%- if product != blank -%}
                  <div class="product__vendor element-spacing-all{% if settings.animations_reveal_on_scroll %} scroll-trigger animate--fade-in{% endif %}" 
                    {{ block.shopify_attributes }}
                    {% if settings.animations_reveal_on_scroll %}
                      style="--animation-order: {{ forloop.index }};
                        --padding-top: {{ block.settings.padding_top }}px;
                        --padding-bottom: {{ block.settings.padding_bottom }}px;"
                    {% else %}
                      style="--padding-top: {{ block.settings.padding_top }}px;
                        --padding-bottom: {{ block.settings.padding_bottom }}px;"
                    {% endif %}
                  >
                    {%- assign vendor_url = product.vendor | url_for_vendor -%}
                    {%- if section.settings.gallery_layout != 'slider_thumbnail' -%}
                      {{ 'products.product.vendor_prefix' | t }}
                    {%- endif -%}
                    {{ product.vendor | link_to: vendor_url, class: 'link u-link u-u' }}
                  </div>
                {% endif %}
              {%- when 'price' -%}
                <div class="product__price element-spacing-all no-js-hidden{% if settings.animations_reveal_on_scroll %} scroll-trigger animate--fade-in{% endif %}" 
                  {{ block.shopify_attributes }}
                  id="price-{{ section.id }}" role="status" 
                  {% if settings.animations_reveal_on_scroll %}
                    style="--animation-order: {{ forloop.index }};
                      --padding-top: {{ block.settings.padding_top }}px;
                      --padding-bottom: {{ block.settings.padding_bottom }}px;"
                  {% else %}
                    style="--padding-top: {{ block.settings.padding_top }}px;
                      --padding-bottom: {{ block.settings.padding_bottom }}px;"
                  {% endif %}
                >
                  {%- liquid
                    if section.settings.type_style == 'special' or section.settings.type_style == 'modern'
                      assign show_you_save = false
                    else
                      assign show_you_save = true
                    endif
                  -%}
                  {%- render 'price',
                    product: product,
                    use_variant: true,
                    show_you_save: show_you_save,
                    main_price: true,
                    price_class: 'price--large style-m-price'
                  -%}
                </div>
                {%- if block.settings.show_taxes or block.settings.show_shipping -%}
                  {%- if shop.taxes_included or shop.shipping_policy.body != blank -%}
                    <div
                      class="product__tax caption rte{% if settings.animations_reveal_on_scroll %} scroll-trigger animate--fade-in{% endif %}{% if block.settings.use_custom_color %} gradient{% endif %}"
                      style="
                        {%- if settings.animations_reveal_on_scroll %}
                          --animation-order: {{ forloop.index }};
                        {% endif -%}
                        {%- if block.settings.use_custom_color %}
                          --color-foreground: {{ block.settings.colors_text.red }}, {{ block.settings.colors_text.green }}, {{ block.settings.colors_text.blue }};
                        {% endif -%}
                      "
                    >
                      {%- if shop.taxes_included and block.settings.show_taxes -%}
                        {{ 'products.product.include_taxes' | t }}
                      {%- endif -%}
                      {%- if shop.shipping_policy.body != blank and block.settings.show_shipping -%}
                        {{ 'products.product.shipping_policy_html' | t: link: shop.shipping_policy.url }}
                      {%- endif -%}
                    </div>
                  {%- endif -%}
                {%- endif -%}
                {%- if product != blank -%}
                  <div {% if settings.animations_reveal_on_scroll %}class="scroll-trigger animate--fade-in"{% endif %} 
                    {{ block.shopify_attributes }}
                    {% if settings.animations_reveal_on_scroll %}
                      style="--animation-order: {{ forloop.index }};"
                    {% endif %}
                  >
                    {%- assign product_form_installment_id = 'product-form-installment-' | append: section.id -%}
                    {%- form 'product', product, id: product_form_installment_id, class: 'installment caption-large' -%}
                      <input type="hidden" name="id" value="{{ product.selected_or_first_available_variant.id }}">
                      {{ form | payment_terms }}
                    {%- endform -%}
                  </div>
                {% endif %}
              {%- when 'information' -%}
                {%- if block.settings.show_sku or block.settings.show_availability or block.settings.show_type or block.settings.show_shipping -%}
                  <div class="product__information element-spacing-all{% if settings.animations_reveal_on_scroll %} scroll-trigger animate--fade-in{% endif %}" 
                    {{ block.shopify_attributes }}
                    {% if settings.animations_reveal_on_scroll %}
                      style="--animation-order: {{ forloop.index }};
                        --padding-top: {{ block.settings.padding_top }}px;
                        --padding-bottom: {{ block.settings.padding_bottom }}px;"
                    {% else %}
                      style="--padding-top: {{ block.settings.padding_top }}px;
                        --padding-bottom: {{ block.settings.padding_bottom }}px;"
                    {% endif %}
                  >
                    <div class="product__info--wrapper d-table">
                      {%- if block.settings.show_sku -%}
                        <div class="product__info--item d-table-r" {% unless product.selected_or_first_available_variant.sku != blank %} style="display: none;"{% endunless %}>
                          <span class="product__info--name">
                            {{ 'products.product.information.sku' | t }}
                          </span>
                          <span class="product__info--value" id="Sku-{{ section.id }}" role="status">
                            {{ product.selected_or_first_available_variant.sku }}
                            <span class="visually-hidden">{{ 'products.product.sku' | t }}:</span>
                            {{- product.selected_or_first_available_variant.sku -}}
                          </span>
                        </div>
                      {%- endif -%}
                      {%- if block.settings.show_availability -%}
                        <div class="product__info--item d-table-r">
                          <span class="product__info--name">
                            {{ 'products.product.information.availability' | t }}
                          </span>
                          <span class="product__info--value" data-inventory>
                            {%- if product.selected_or_first_available_variant.inventory_management -%}
                              {%- assign first_inventory = product.selected_or_first_available_variant.inventory_quantity -%}
                              {%- if first_inventory > 0 -%}
                                {{ 'products.product.information.in_stock' | t }}
                              {%- else -%}
                                {{ 'products.product.information.out_of_stock' | t }}
                              {%- endif -%}
                            {%- else -%}
                              {{ 'products.product.information.many_in_stock' | t }}
                            {%- endif -%}
                          </span>
                        </div>
                      {%- endif -%}
                      {%- if block.settings.show_type and product.type != blank -%}
                        <div class="product__info--item d-table-r">
                          <span class="product__info--name">
                            {{ 'products.product.information.type' | t }}
                          </span>
                          <span class="product__info--value u-link">
                            {{ product.type | escape | link_to_type }}
                          </span>
                        </div>
                      {%- endif -%}
                      {%- if block.settings.show_shipping -%}
                        {%- liquid
                          assign link = pages['shipping-returns'].url
                          if link == blank
                            assign link = shop.shipping_policy.url
                          endif
                          assign link_text = 'products.product.information.shipping' | t
                        -%}
                        <div class="product__info--item d-table-r">
                          <span class="product__info--name">
                            <a href="{{ link }}" class="link u-link u-u link--style-line f-bold">
                              <span class="link-style">
                                {{- link_text -}}
                              </span>
                            </a>
                          </span>
                          <span class="product__info--value">
                            {{ 'products.product.information.shipping_text' | t }}
                          </span>
                        </div>
                      {%- endif -%}
                    </div>
                  </div>
                {%- endif -%}
              {%- when 'inventory' -%}
                <div
                  class="product__inventory no-js-hidden element-spacing-all"
                  {{ block.shopify_attributes }}
                  id="Inventory-{{ section.id }}"
                  role="status"
                  style="
                    --padding-top: {{ block.settings.padding_top }}px;
                  --padding-bottom: {{ block.settings.padding_bottom }}px;
                  --color-foreground: {{ block.settings.colors_text.red }}, {{ block.settings.colors_text.green }}, {{ block.settings.colors_text.blue }};
                  --color-icon: {{ block.settings.colors_icon.red }}, {{ block.settings.colors_icon.green }}, {{ block.settings.colors_icon.blue }};
                  --color-background: {{ block.settings.colors_background.red }}, {{ block.settings.colors_background.green }}, {{ block.settings.colors_background.blue }};
                  --gradient-background: {% if block.settings.gradient_background != blank %}{{ block.settings.gradient_background }}{% else %}{{ block.settings.colors_background }}{% endif %};
                  "
                >
                  {%- if product.selected_or_first_available_variant.inventory_management == 'shopify' -%}
                    {%- if product.selected_or_first_available_variant.inventory_quantity > 0 -%}
                      {%- if block.settings.show_inventory_label -%}
                        <div class="product__inventory--text">
                          {%- if product.selected_or_first_available_variant.inventory_quantity <= block.settings.inventory_threshold -%}
                            {{- 'products.product.inventory_low_stock' | t -}}
                          {%- else -%}
                            {{- 'products.product.inventory_in_stock' | t -}}
                          {%- endif -%}
                        </div>
                      {%- endif -%}
                      {%- liquid
                        assign percent = product.selected_or_first_available_variant.inventory_quantity | times: 100 | divided_by: block.settings.inventory_threshold | round: 2
                        if percent > 100
                          assign percent = 100
                        endif
                      -%}
                      <div
                        class="product__inventory--progress d-block w-100 pos-relative{% if product.selected_or_first_available_variant.inventory_management != 'shopify' %} visibility-hidden{% endif %}"
                        style="--percent: {{ percent }}%;"
                      >
                        <span class="visually-hidden"></span>
                        <div
                          class="product__inventory--inventory d-flex a-i-center{% if block.settings.inventory_text == 'short' %} pos-absolute{% else %} --standard{% endif %} g-gap z-index-1"
                          style="--g-gap: 1rem;"
                        >
                          {%- if block.settings.inventory_text == 'short' -%}
                            <svg
                              class="icon icon-tick"
                              aria-hidden="true"
                              focusable="false"
                              xmlns="http://www.w3.org/2000/svg"
                              viewBox="0 0 12 9"
                              fill="none"
                            >
                              <path fill-rule="evenodd" clip-rule="evenodd" d="M11.35.643a.5.5 0 01.006.707l-6.77 6.886a.5.5 0 01-.719-.006L.638 4.845a.5.5 0 11.724-.69l2.872 3.011 6.41-6.517a.5.5 0 01.707-.006h-.001z" fill="currentColor"/>
                            </svg>
                          {%- endif -%}
                          {%- if product.selected_or_first_available_variant.inventory_quantity <= block.settings.inventory_threshold -%}
                              {%- if block.settings.inventory_text == 'short' -%}
                              {{- 'products.product.inventory_low_stock_show_count'
                                | t: quantity: product.selected_or_first_available_variant.inventory_quantity
                              -}}
                            {%- endif -%}
                          {%- else -%}
                            {%- if block.settings.inventory_text == 'short' -%}
                              {{- 'products.product.inventory_in_stock_show_count' | t: quantity: product.selected_or_first_available_variant.inventory_quantity -}}
                            {%- endif -%}
                          {%- endif -%}
                        </div>
                      </div>
                      {%- if product.selected_or_first_available_variant.inventory_quantity
                          <= block.settings.inventory_threshold
                      -%}
                        {%- if block.settings.inventory_text == 'standard' -%}
                          <div class="product__inventory--left">
                            Please Hurry, Only
                            <span class="f-bold">
                              {{
                                'products.product.inventory_low_stock_show_count__2'
                                | t: quantity: product.selected_or_first_available_variant.inventory_quantity
                              }}
                            </span>
                            Items Left In Stock!
                          </div>
                        {%- endif -%}
                      {%- else -%}
                        {%- if block.settings.inventory_text == 'standard' -%}
                          <div class="product__inventory--left">
                            {{-
                              'products.product.inventory_in_stock_show_count'
                              | t: quantity: product.selected_or_first_available_variant.inventory_quantity
                            -}}
                          </div>
                        {%- endif -%}
                      {%- endif -%}
                    {%- else -%}
                      {%- if product.selected_or_first_available_variant.inventory_policy == 'continue' -%}
                        <div class="product__inventory--status d-flex a-i-center g-gap" style="--g-gap: 1rem;">
                          <svg width="15" height="15" aria-hidden="true" class="icon">
                            <circle cx="7.5" cy="7.5" r="7.5" fill="rgb(62,214,96, 0.3)"/>
                            <circle cx="7.5" cy="7.5" r="5" stroke="rgb(255, 255, 255)" stroke-width="1" fill="rgb(62,214,96)"/>
                          </svg>
                          {{- 'products.product.inventory_out_of_stock_continue_selling' | t -}}
                        </div>
                      {%- else -%}
                        <div class="product__inventory--status d-flex a-i-center g-gap" style="--g-gap: 1rem;">
                          <svg width="15" height="15" aria-hidden="true" class="icon">
                            <circle cx="7.5" cy="7.5" r="7.5" fill="rgb(200,200,200, 0.3)"/>
                            <circle cx="7.5" cy="7.5" r="5" stroke="rgb(255, 255, 255)" stroke-width="1" fill="rgb(200,200,200)"/>
                          </svg>
                          {{- 'products.product.inventory_out_of_stock' | t -}}
                        </div>
                      {%- endif -%}
                    {%- endif -%}
                  {%- endif -%}
                  {%- if block.settings.show_shipping -%}
                    {%- liquid
                      assign link = pages['shipping-returns'].url
                      if link == blank
                        assign link = shop.shipping_policy.url
                      endif
                      assign link_text = 'products.product.information.shipping' | t
                    -%}
                    <div class="product__inventory--shipping m-t uppercase" style="--m-t: 1.6rem;">
                      <div class="d-table-r">
                        <span class="product__inventory--name">
                          {% comment %} {{ link_text | link_to: link, class: 'link u-link u-u link-style f-bold' }} {% endcomment %}
                          <a href="{{ link }}" class="link u-link u-u link--style-line f-bold">
                            <span class="link-style">
                              {{- link_text -}}
                            </span>
                          </a>
                        </span>
                        <span class="product__inventory--value">
                          {{ 'products.product.information.shipping_text' | t }}
                        </span>
                      </div>
                    </div>
                  {%- endif -%}
                  {%- if block.settings.show_availability -%}
                    <div class="product__inventory--available d-flex a-i-center g-gap m-t" style="--g-gap: 1rem;--m-t: 1.2rem;">
                      <svg
                        class="icon icon-tick"
                        aria-hidden="true"
                        focusable="false"
                        xmlns="http://www.w3.org/2000/svg"
                        viewBox="0 0 12 9"
                        fill="none"
                      >
                        <path fill-rule="evenodd" clip-rule="evenodd" d="M11.35.643a.5.5 0 01.006.707l-6.77 6.886a.5.5 0 01-.719-.006L.638 4.845a.5.5 0 11.724-.69l2.872 3.011 6.41-6.517a.5.5 0 01.707-.006h-.001z" fill="currentColor"/>
                      </svg>
                      {%- if product.selected_or_first_available_variant.inventory_quantity
                          <= block.settings.inventory_threshold
                      -%}
                        {{- 'products.product.inventory_low_stock' | t -}}
                      {%- else -%}
                        {{- 'products.product.inventory_in_stock' | t -}}
                      {%- endif -%}
                    </div>
                  {%- endif -%}
                </div>
              {%- when 'countdown' -%}
                {%- if block.settings.time != blank -%}
                  <div class="product__countdown color-foreground element-spacing-all{% if settings.animations_reveal_on_scroll %} scroll-trigger animate--fade-in{% endif %}" 
                    {{ block.shopify_attributes }}
                    {% if settings.animations_reveal_on_scroll %}
                      style="--animation-order: {{ forloop.index }};
                        --padding-top: {{ block.settings.padding_top }}px;
                        --padding-bottom: {{ block.settings.padding_bottom }}px;
                        --color-foreground: {{ block.settings.colors_text.red }}, {{ block.settings.colors_text.green }}, {{ block.settings.colors_text.blue }}"
                    {% else %}
                      style="--padding-top: {{ block.settings.padding_top }}px;
                        --padding-bottom: {{ block.settings.padding_bottom }}px;
                        --color-foreground: {{ block.settings.colors_text.red }}, {{ block.settings.colors_text.green }}, {{ block.settings.colors_text.blue }}"
                    {% endif %}
                  >
                    <count-down class="d-block" data-countdown="{{ block.settings.time }}" data-type="product">
                      {%- if block.settings.text != blank -%}
                        <div class="info d-flex a-i-center">
                          <svg class="icon" aria-hidden="true" focusable="false" role="presentation" xmlns="http://www.w3.org/2000/svg" viewBox="0 0 20 20">
