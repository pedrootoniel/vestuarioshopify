                          --color-foreground: {{ block.settings.colors_icon.red }}, {{ block.settings.colors_icon.green }}, {{ block.settings.colors_icon.blue }};
                          --color-background: {{ block.settings.colors_icon_background.red }}, {{ block.settings.colors_icon_background.green }}, {{ block.settings.colors_icon_background.blue }};
                        "
