                              <button
                                id="ProductPopup-{{ block.id }}"
                                class="button button--main-standard {{ settings.button_style | prepend: 'button--style-' }}{% if block.settings.button_type == 'transparent' %} button--transparent{% endif %}"
                                type="button"
                                aria-haspopup="dialog"
                              >
                                <span>{{ block.settings.button_label | escape }}</span>
                              </button>
