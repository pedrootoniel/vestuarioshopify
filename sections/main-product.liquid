                  <div class="product__size-chart color-{{ block.settings.color_size_chart_scheme }} gradient element-spacing-all{% if settings.animations_reveal_on_scroll %} scroll-trigger animate--fade-in{% endif %}" {{ block.shopify_attributes }}
