                              --color-text: {{ block.settings.color_text.red }}, {{ block.settings.color_text.green }}, {{ block.settings.color_text.blue }};
                              {% if settings.animations_reveal_on_scroll %} --animation-order: {{ i | plus: 1 }};{% endif %}
                            "
                          >
                            <div class="product__quickview-inner d-flex a-i-center" data-customer-viewing data-customer-view="{{ block.settings.customer_viewing_number }}" data-customer-view-time="{{ block.settings.customer_viewing_time }}">
                              {% render 'icon-positive', type: block.settings.type_icon %}
                              <span class="text">{{ 'products.product.customer_viewing' | t: number: '10' }}</span>
                            </div>
                          </customer-viewing>
                        </div>
                      {%- endif -%}
                      {%- for i in (1..4) -%}
                        {%- capture cusText -%}cus_text_{{ i }}{%- endcapture -%}
                        {%- capture typeIcon -%}type_icon_{{ i }}{%- endcapture -%}
                        {%- capture colorText -%}color_text_{{ i }}{%- endcapture -%}
                        {%- liquid 
                          assign cus_text = block.settings[cusText]
                          assign type_icon = block.settings[typeIcon]
                          assign color_text = block.settings[colorText]
                        -%}
                        {%- if cusText != blank -%}
                          <div role="listitem" id="Slide-{{ block.id }}-{{ i | plus: 1 }}" class="carousel__item swiper-slide">
                            <div class="product__custext product__quickview{% if settings.animations_reveal_on_scroll %} scroll-trigger animate--fade-in{% endif %}"
                              style="
                                --color-text: {{ color_text.red }}, {{ color_text.green }}, {{ color_text.blue }};
                                {% if settings.animations_reveal_on_scroll %} --animation-order: {{ i | plus: 1 }};{% endif %}
                              "
                            >
                              <div class="product__custext-inner d-flex a-i-center">
                                {% render 'icon-positive', type: type_icon %}
                                <span class="text">{{ cus_text }}</span>
                              </div>
                            </div>
                          </div>
                        {%- endif -%}
                      {%- endfor -%}
                    </div>
                  </div>
                </slider-component>
              {%- when 'ask_an_expert' -%}
                <div class="product__quickview-ask element-spacing-all color-{{ block.settings.button_color_scheme }}{% if settings.animations_reveal_on_scroll %} scroll-trigger animate--fade-in{% endif %}" {{ block.shopify_attributes }}
