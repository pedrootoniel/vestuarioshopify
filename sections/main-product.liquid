                      <h2 class="{{ block.settings.heading_size | prepend: 'text-size text-size-' }} title m-0 d-inline-block break {{ block.settings.heading_font }}">
                        {{ block.settings.heading | default: block.settings.page.title }}
                      </h2>
                    </div>
                    <div class="ask-an-exp__button">
                      {%- if block.settings.button_label != blank -%}
                        <div class="o-h m-spacing m-spacing-xs">
