<section
  id="MainProduct-{{ section.id }}"
  data-section="{{ section.id }}"
  class="section__m-product spacing spacing-xs color-{{ section.settings.color_scheme }} gradient pos-relative"
  style="
    --padding-top: {{ section.settings.padding_top }}px;
    --padding-bottom: {{ section.settings.padding_bottom }}px;
    --padding-top-xs: {{ section.settings.padding_top_mobile }}px;
    --padding-bottom-xs: {{ section.settings.padding_bottom_mobile }}px;
  "
>
  {%- liquid
    assign first_3d_model = product.media | where: 'media_type', 'model' | first
    capture icon_caret
      render 'icon-caret'
    endcapture
  -%}
  {{ 'section-main-product.css' | asset_url | stylesheet_tag }}
  {%- if first_3d_model -%}
    <link id="ModelViewerStyle" rel="stylesheet" href="https://cdn.shopify.com/shopifycloud/model-viewer-ui/assets/v1.0/model-viewer-ui.css" media="print" onload="this.media='all'">
    <link id="ModelViewerOverride" rel="stylesheet" href="{{ 'component-model-viewer-ui.css' | asset_url }}" media="print" onload="this.media='all'">
  {%- endif -%}
  <div class="page-width m-auto">
    {% render 'breadcrumb',
      padding_top: '2rem',
      padding_bottom: '4rem',
      alignment: 'start'
    %}
    <div class="product product--{{ section.settings.media_size }} product--{{ section.settings.gallery_layout }} product--style-{{ section.settings.type_style }} d-flex f-wrap{% if product.media.size == 0 %} product--no-media m-auto{% endif %} r-gap" style="--r-gap: 3rem;">
      {%- if product.media.size > 0 -%}
        <div class="product__media-wrapper w-100 pos-relative f-grow f-shrink">
          {% render 'product-media-gallery',
            product: product,
            gallery_layout: section.settings.gallery_layout,
            media_size: section.settings.media_size,
            enable_video_looping: section.settings.enable_video_looping,
            type_style: section.settings.type_style,
            show_product_badges: section.settings.show_product_badges,
            carousel_direction: section.settings.carousel_direction,
            carousel_gap_tablet: section.settings.carousel_gap_tablet,
            carousel_gap_desktop: section.settings.carousel_gap_desktop,
            ratio_square: section.settings.image_ratio
          %}
          {%- if section.settings.show_product_badges and section.settings.type_style != 'special' -%}
            {%- liquid
              if settings.badge_position == 'top left'
                assign position_class = 'top-0 left-0'
              else
                assign position_class = 'top-0 right-0'
              endif

              if settings.sale_badge_type == 'discount'
                assign current_variant = product.selected_or_first_available_variant
                if current_variant.compare_at_price > current_variant.price
                    assign product_discount_price = current_variant.compare_at_price | minus: current_variant.price | times: 100.0 | divided_by: current_variant.compare_at_price | round | append: '%' | prepend: '-'
                endif
              endif
            -%}
            <div class="product__badge pos-absolute z-index-1 d-flex f-column {{ settings.badge_position }} {{ settings.badge_transform }} {{ position_class }}">
              {%- if product.compare_at_price > product.price and product.available -%}
                <span id="Badge-{{ section.id }}-{{ product.id }}" 
                  class="badge sale-badge d-inline-block center break {{ settings.badge_button_type }}"
                >
                  {%- if settings.sale_badge_type == 'discount' -%}
                    {{ product_discount_price }}
                  {%- else -%}
                    {{- 'products.product.on_sale' | t -}}
                  {%- endif -%}
                </span>
              {%- endif -%}
              {%- if product.available == false -%}
                <span id="Badge-{{ section.id }}-{{ product.id }}" 
                  class="badge sold-out-badge d-inline-block center break {{ settings.badge_button_type }}"
                >
                  {{- 'products.product.sold_out' | t -}}
                </span>
              {%- endif -%}
            </div>
          {%- endif -%}
          {%- if section.settings.enable_share and section.settings.type_style == 'classic' -%}
            <div class="product__share pos-absolute z-index-2">
              {%- liquid
                assign share_label = 'products.product.share' | t
                assign share_url = product.selected_variant.url | default: product.url | prepend: request.origin
              -%}
              {% render 'share-button',
                section: section,
                share_label: share_label,
                share_link: share_url
              %}
            </div>
          {%- endif -%}
        </div>
      {%- endif -%}
      <div class="product__info-wrapper w-100 f-grow f-shrink product-view-{{ section.settings.type_style }}{% if section.settings.type_style == 'special' %} pos-relative{% endif %}{% if settings.animations_reveal_on_scroll %} scroll-trigger animate--fade-in{% endif %}">
        <product-info
          id="ProductInfo-{{ section.id }}"
          data-section="{{ section.id }}"
          data-url="{{ product.url }}"
          class="product__info-container d-block"
        >
          {%- if section.settings.enable_share -%}
            {%- if section.settings.type_style != 'classic' -%}
              <div class="product__share pos-absolute z-index-2 top-0 right-0">
                {%- liquid
                  assign share_label = 'products.product.share' | t
                  assign share_url = product.selected_variant.url | default: product.url | prepend: request.origin
                -%}
                {% render 'share-button', section: section, share_label: share_label, share_link: share_url %}
              </div>
            {%- endif -%}
          {%- endif -%}
          {%- assign product_form_id = 'product-form-' | append: section.id -%}
          {%- for block in section.blocks -%}
            {%- case block.type -%}
              {%- when '@app' -%}
                {% render block %}
              {%- when 'text' -%}
                <p class="product__text element-spacing-all{% if settings.animations_reveal_on_scroll %} scroll-trigger animate--fade-in{% endif %}" 
                  {{ block.shopify_attributes }}
                  {% if settings.animations_reveal_on_scroll %}
                    style="--animation-order: {{ forloop.index }};
                      --padding-top: {{ block.settings.padding_top }}px;
                      --padding-bottom: {{ block.settings.padding_bottom }}px;"
                  {% else %}
                    style="--padding-top: {{ block.settings.padding_top }}px;
                      --padding-bottom: {{ block.settings.padding_bottom }}px;"
                  {% endif %}
                >
                  {{ block.settings.text }}
                </p>
              {%- when 'title' -%}
                <div class="product__title break element-spacing-all{% if settings.animations_reveal_on_scroll %} scroll-trigger animate--fade-in{% endif %}" 
                  {{ block.shopify_attributes }}
                  {% if settings.animations_reveal_on_scroll %}
                    style="--animation-order: {{ forloop.index }};
                      --padding-top: {{ block.settings.padding_top }}px;
                      --padding-bottom: {{ block.settings.padding_bottom }}px;"
                  {% else %}
                    style="--padding-top: {{ block.settings.padding_top }}px;
                      --padding-bottom: {{ block.settings.padding_bottom }}px;"
                  {% endif %}
                >
                  <h1 class="{% if block.settings.heading_size == 'h1' or block.settings.heading_size == 'h2' %} {{ block.settings.heading_size }}{%  else %}p-w__heading {{ block.settings.heading_size | prepend: 'heading-size-' }}{% endif %} m-0">
                    {{ product.title | escape }}
                  </h1>
                </div>
              {%- when 'vendor' -%}
                <div class="product__vendor element-spacing-all{% if settings.animations_reveal_on_scroll %} scroll-trigger animate--fade-in{% endif %}" 
                  {{ block.shopify_attributes }}
                  {% if settings.animations_reveal_on_scroll %}
                    style="--animation-order: {{ forloop.index }};
                      --padding-top: {{ block.settings.padding_top }}px;
                      --padding-bottom: {{ block.settings.padding_bottom }}px;"
                  {% else %}
                    style="--padding-top: {{ block.settings.padding_top }}px;
                      --padding-bottom: {{ block.settings.padding_bottom }}px;"
                  {% endif %}
                >
                  {%- assign vendor_url = product.vendor | url_for_vendor -%}
                  {%- if section.settings.gallery_layout != 'slider_thumbnail' -%}
                    {{ 'products.product.vendor_prefix' | t }}
                  {%- endif -%}
                  {{ product.vendor | link_to: vendor_url, class: 'link u-link u-u' }}
                </div>
              {%- when 'price' -%}
                <div class="product__price element-spacing-all no-js-hidden{% if settings.animations_reveal_on_scroll %} scroll-trigger animate--fade-in{% endif %}" 
                  {{ block.shopify_attributes }}
                  id="price-{{ section.id }}" role="status" 
                  {% if settings.animations_reveal_on_scroll %}
                    style="--animation-order: {{ forloop.index }};
                      --padding-top: {{ block.settings.padding_top }}px;
                      --padding-bottom: {{ block.settings.padding_bottom }}px;"
                  {% else %}
                    style="--padding-top: {{ block.settings.padding_top }}px;
                      --padding-bottom: {{ block.settings.padding_bottom }}px;"
                  {% endif %}
                >
                  {%- liquid
                    if section.settings.type_style == 'special' or section.settings.type_style == 'modern'
                      assign show_you_save = false
                    else
                      assign show_you_save = true
                    endif
                  -%}
                  {%- render 'price',
                    product: product,
                    use_variant: true,
                    show_you_save: show_you_save,
                    main_price: true,
                    price_class: 'price--large style-m-price'
                  -%}
                </div>
                {%- if block.settings.show_taxes or block.settings.show_shipping -%}
                  {%- if shop.taxes_included or shop.shipping_policy.body != blank -%}
                    <div
                      class="product__tax caption rte{% if settings.animations_reveal_on_scroll %} scroll-trigger animate--fade-in{% endif %}{% if block.settings.use_custom_color %} gradient{% endif %}"
                      style="
                        {%- if settings.animations_reveal_on_scroll %}
                          --animation-order: {{ forloop.index }};
                        {% endif -%}
                        {%- if block.settings.use_custom_color %}
                          --color-foreground: {{ block.settings.colors_text.red }}, {{ block.settings.colors_text.green }}, {{ block.settings.colors_text.blue }};
                        {% endif -%}
                      "
                    >
                      {%- if shop.taxes_included and block.settings.show_taxes -%}
                        {{ 'products.product.include_taxes' | t }}
                      {%- endif -%}
                      {%- if shop.shipping_policy.body != blank and block.settings.show_shipping -%}
                        {{ 'products.product.shipping_policy_html' | t: link: shop.shipping_policy.url }}
                      {%- endif -%}
                    </div>
                  {%- endif -%}
                {%- endif -%}
                <div {% if settings.animations_reveal_on_scroll %}class="scroll-trigger animate--fade-in"{% endif %} 
                  {{ block.shopify_attributes }}
                  {% if settings.animations_reveal_on_scroll %}
                    style="--animation-order: {{ forloop.index }};"
                  {% endif %}
                >
                  {%- assign product_form_installment_id = 'product-form-installment-' | append: section.id -%}
                  {%- form 'product', product, id: product_form_installment_id, class: 'installment caption-large' -%}
                    <input type="hidden" name="id" value="{{ product.selected_or_first_available_variant.id }}">
                    {{ form | payment_terms }}
                  {%- endform -%}
                </div>
              {%- when 'information' -%}
                {%- if block.settings.show_sku or block.settings.show_availability or block.settings.show_type or block.settings.show_shipping -%}
                  <div class="product__information element-spacing-all{% if settings.animations_reveal_on_scroll %} scroll-trigger animate--fade-in{% endif %}" 
                    {{ block.shopify_attributes }}
                    {% if settings.animations_reveal_on_scroll %}
                      style="--animation-order: {{ forloop.index }};
                        --padding-top: {{ block.settings.padding_top }}px;
                        --padding-bottom: {{ block.settings.padding_bottom }}px;"
                    {% else %}
                      style="--padding-top: {{ block.settings.padding_top }}px;
                        --padding-bottom: {{ block.settings.padding_bottom }}px;"
                    {% endif %}
                  >
                    <div class="product__info--wrapper d-table">
                      {%- if block.settings.show_sku -%}
                        <div class="product__info--item d-table-r" {% unless product.selected_or_first_available_variant.sku != blank %} style="display: none;"{% endunless %}>
                          <span class="product__info--name">
                            {{ 'products.product.information.sku' | t }}
                          </span>
                          <span class="product__info--value" id="Sku-{{ section.id }}" role="status">
                            {{ product.selected_or_first_available_variant.sku }}
                          </span>
                        </div>
                      {%- endif -%}
                      {%- if block.settings.show_availability -%}
                        <div class="product__info--item d-table-r">
                          <span class="product__info--name">
                            {{ 'products.product.information.availability' | t }}
                          </span>
                          <span class="product__info--value" data-inventory>
                            {%- if product.selected_or_first_available_variant.inventory_management -%}
                              {%- assign first_inventory = product.selected_or_first_available_variant.inventory_quantity -%}
                              {%- if first_inventory > 0 -%}
                                {{ 'products.product.information.in_stock' | t }}
                              {%- else -%}
                                {{ 'products.product.information.out_of_stock' | t }}
                              {%- endif -%}
                            {%- else -%}
                              {{ 'products.product.information.many_in_stock' | t }}
                            {%- endif -%}
                          </span>
                        </div>
                      {%- endif -%}
                      {%- if block.settings.show_type and product.type != blank -%}
                        <div class="product__info--item d-table-r">
                          <span class="product__info--name">
                            {{ 'products.product.information.type' | t }}
                          </span>
                          <span class="product__info--value u-link">
                            {{ product.type | escape | link_to_type }}
                          </span>
                        </div>
                      {%- endif -%}
                      {%- if block.settings.show_shipping -%}
                        {%- liquid
                          assign link = pages['shipping-returns'].url
                          if link == blank
                            assign link = shop.shipping_policy.url
                          endif
                          assign link_text = 'products.product.information.shipping' | t
                        -%}
                        <div class="product__info--item d-table-r">
                          <span class="product__info--name">
                            <a href="{{ link }}" class="link u-link u-u link--style-line f-bold">
                              <span class="link-style">
                                {{- link_text -}}
                              </span>
                            </a>
                          </span>
                          <span class="product__info--value">
                            {{ 'products.product.information.shipping_text' | t }}
                          </span>
                        </div>
                      {%- endif -%}
                    </div>
                  </div>
                {%- endif -%}
              {%- when 'inventory' -%}
                <div
                  class="product__inventory no-js-hidden element-spacing-all"
                  {{ block.shopify_attributes }}
                  id="Inventory-{{ section.id }}"
                  role="status"
                  style="
                    --padding-top: {{ block.settings.padding_top }}px;
                  --padding-bottom: {{ block.settings.padding_bottom }}px;
                  --color-foreground: {{ block.settings.colors_text.red }}, {{ block.settings.colors_text.green }}, {{ block.settings.colors_text.blue }};
                  --color-icon: {{ block.settings.colors_icon.red }}, {{ block.settings.colors_icon.green }}, {{ block.settings.colors_icon.blue }};
                  --color-background: {{ block.settings.colors_background.red }}, {{ block.settings.colors_background.green }}, {{ block.settings.colors_background.blue }};
                  --gradient-background: {% if block.settings.gradient_background != blank %}{{ block.settings.gradient_background }}{% else %}{{ block.settings.colors_background }}{% endif %};
                  "
                >
                  {%- if product.selected_or_first_available_variant.inventory_management == 'shopify' -%}
                    {%- if product.selected_or_first_available_variant.inventory_quantity > 0 -%}
                      {%- if block.settings.show_inventory_label -%}
                        <div class="product__inventory--text">
                          {%- if product.selected_or_first_available_variant.inventory_quantity <= block.settings.inventory_threshold -%}
                            {{- 'products.product.inventory_low_stock' | t -}}
                          {%- else -%}
                            {{- 'products.product.inventory_in_stock' | t -}}
                          {%- endif -%}
                        </div>
                      {%- endif -%}
                      {%- liquid
                        assign percent = product.selected_or_first_available_variant.inventory_quantity | times: 100 | divided_by: block.settings.inventory_threshold | round: 2
                        if percent > 100
                          assign percent = 100
                        endif
                      -%}
                      <div
                        class="product__inventory--progress d-block w-100 pos-relative{% if product.selected_or_first_available_variant.inventory_management != 'shopify' %} visibility-hidden{% endif %}"
                        style="--percent: {{ percent }}%;"
                      >
                        <span class="visually-hidden"></span>
                        <div
                          class="product__inventory--inventory d-flex a-i-center{% if block.settings.inventory_text == 'short' %} pos-absolute{% else %} --standard{% endif %} g-gap z-index-1"
                          style="--g-gap: 1rem;"
                        >
                          {%- if block.settings.inventory_text == 'short' -%}
                            <svg
                              class="icon icon-tick"
                              aria-hidden="true"
                              focusable="false"
                              xmlns="http://www.w3.org/2000/svg"
                              viewBox="0 0 12 9"
                              fill="none"
                            >
                              <path fill-rule="evenodd" clip-rule="evenodd" d="M11.35.643a.5.5 0 01.006.707l-6.77 6.886a.5.5 0 01-.719-.006L.638 4.845a.5.5 0 11.724-.69l2.872 3.011 6.41-6.517a.5.5 0 01.707-.006h-.001z" fill="currentColor"/>
                            </svg>
                          {%- endif -%}
                          {%- if product.selected_or_first_available_variant.inventory_quantity <= block.settings.inventory_threshold -%}
                              {%- if block.settings.inventory_text == 'short' -%}
                              {{- 'products.product.inventory_low_stock_show_count'
                                | t: quantity: product.selected_or_first_available_variant.inventory_quantity
                              -}}
                            {%- endif -%}
                          {%- else -%}
                            {%- if block.settings.inventory_text == 'short' -%}
                              {{- 'products.product.inventory_in_stock_show_count' | t: quantity: product.selected_or_first_available_variant.inventory_quantity -}}
                            {%- endif -%}
                          {%- endif -%}
                        </div>
                      </div>
                      {%- if product.selected_or_first_available_variant.inventory_quantity
                          <= block.settings.inventory_threshold
                      -%}
                        {%- if block.settings.inventory_text == 'standard' -%}
                          <div class="product__inventory--left">
                            Please Hurry, Only
                            <span class="f-bold">
                              {{
                                'products.product.inventory_low_stock_show_count__2'
                                | t: quantity: product.selected_or_first_available_variant.inventory_quantity
                              }}
                            </span>
                            Items Left In Stock!
                          </div>
                        {%- endif -%}
                      {%- else -%}
                        {%- if block.settings.inventory_text == 'standard' -%}
                          <div class="product__inventory--left">
                            {{-
                              'products.product.inventory_in_stock_show_count'
                              | t: quantity: product.selected_or_first_available_variant.inventory_quantity
                            -}}
                          </div>
                        {%- endif -%}
                      {%- endif -%}
                    {%- else -%}
                      {%- if product.selected_or_first_available_variant.inventory_policy == 'continue' -%}
                        <div class="product__inventory--status d-flex a-i-center g-gap" style="--g-gap: 1rem;">
                          <svg width="15" height="15" aria-hidden="true" class="icon">
                            <circle cx="7.5" cy="7.5" r="7.5" fill="rgb(62,214,96, 0.3)"/>
                            <circle cx="7.5" cy="7.5" r="5" stroke="rgb(255, 255, 255)" stroke-width="1" fill="rgb(62,214,96)"/>
                          </svg>
                          {{- 'products.product.inventory_out_of_stock_continue_selling' | t -}}
                        </div>
                      {%- else -%}
                        <div class="product__inventory--status d-flex a-i-center g-gap" style="--g-gap: 1rem;">
                          <svg width="15" height="15" aria-hidden="true" class="icon">
                            <circle cx="7.5" cy="7.5" r="7.5" fill="rgb(200,200,200, 0.3)"/>
                            <circle cx="7.5" cy="7.5" r="5" stroke="rgb(255, 255, 255)" stroke-width="1" fill="rgb(200,200,200)"/>
                          </svg>
                          {{- 'products.product.inventory_out_of_stock' | t -}}
                        </div>
                      {%- endif -%}
                    {%- endif -%}
                  {%- endif -%}
                  {%- if block.settings.show_shipping -%}
                    {%- liquid
                      assign link = pages['shipping-returns'].url
                      if link == blank
                        assign link = shop.shipping_policy.url
                      endif
                      assign link_text = 'products.product.information.shipping' | t
                    -%}
                    <div class="product__inventory--shipping m-t uppercase" style="--m-t: 1.6rem;">
                      <div class="d-table-r">
                        <span class="product__inventory--name">
                          {% comment %} {{ link_text | link_to: link, class: 'link u-link u-u link-style f-bold' }} {% endcomment %}
                          <a href="{{ link }}" class="link u-link u-u link--style-line f-bold">
                            <span class="link-style">
                              {{- link_text -}}
                            </span>
                          </a>
                        </span>
                        <span class="product__inventory--value">
                          {{ 'products.product.information.shipping_text' | t }}
                        </span>
                      </div>
                    </div>
                  {%- endif -%}
                  {%- if block.settings.show_availability -%}
                    <div class="product__inventory--available d-flex a-i-center g-gap m-t" style="--g-gap: 1rem;--m-t: 1.2rem;">
                      <svg
                        class="icon icon-tick"
                        aria-hidden="true"
                        focusable="false"
                        xmlns="http://www.w3.org/2000/svg"
                        viewBox="0 0 12 9"
                        fill="none"
                      >
                        <path fill-rule="evenodd" clip-rule="evenodd" d="M11.35.643a.5.5 0 01.006.707l-6.77 6.886a.5.5 0 01-.719-.006L.638 4.845a.5.5 0 11.724-.69l2.872 3.011 6.41-6.517a.5.5 0 01.707-.006h-.001z" fill="currentColor"/>
                      </svg>
                      {%- if product.selected_or_first_available_variant.inventory_quantity
                          <= block.settings.inventory_threshold
                      -%}
                        {{- 'products.product.inventory_low_stock' | t -}}
                      {%- else -%}
                        {{- 'products.product.inventory_in_stock' | t -}}
                      {%- endif -%}
                    </div>
                  {%- endif -%}
                </div>
              {%- when 'countdown' -%}
                {%- if block.settings.time != blank -%}
                  <div class="product__countdown color-foreground element-spacing-all{% if settings.animations_reveal_on_scroll %} scroll-trigger animate--fade-in{% endif %}" 
                    {{ block.shopify_attributes }}
                    {% if settings.animations_reveal_on_scroll %}
                      style="--animation-order: {{ forloop.index }};
                        --padding-top: {{ block.settings.padding_top }}px;
                        --padding-bottom: {{ block.settings.padding_bottom }}px;
                        --color-foreground: {{ block.settings.colors_text.red }}, {{ block.settings.colors_text.green }}, {{ block.settings.colors_text.blue }}"
                    {% else %}
                      style="--padding-top: {{ block.settings.padding_top }}px;
                        --padding-bottom: {{ block.settings.padding_bottom }}px;
                        --color-foreground: {{ block.settings.colors_text.red }}, {{ block.settings.colors_text.green }}, {{ block.settings.colors_text.blue }}"
                    {% endif %}
                  >
                    <count-down class="d-block" data-countdown="{{ block.settings.time }}" data-type="product">
                      {%- if block.settings.text != blank -%}
                        <div class="info d-flex a-i-center">
                          <svg class="icon" aria-hidden="true" focusable="false" role="presentation" xmlns="http://www.w3.org/2000/svg" viewBox="0 0 20 20">
                            <path d="M1.16154 11.8423L8.09186 18.829C8.28991 19.0287 8.61108 19.0284 8.80882 18.8285L18.839 8.68572C18.9337 8.58998 18.9868 8.46032 18.9868 8.32514V1.53212C18.9868 1.25006 18.76 1.02141 18.4802 1.02141L11.387 1.02147C11.2527 1.02147 11.1238 1.07529 11.0288 1.17108L1.16151 11.1201C0.963704 11.3195 0.963715 11.6428 1.16154 11.8423ZM20 1.53212C20 0.685947 19.3196 -6.58649e-06 18.4802 0L11.387 5.82026e-05C10.9839 6.15389e-05 10.5973 0.161509 10.3123 0.448879L0.445049 10.3979C-0.148378 10.9962 -0.148346 11.9662 0.445123 12.5645L7.37544 19.5513C7.96958 20.1502 8.9331 20.1495 9.52633 19.5496L19.5565 9.40686C19.8405 9.11967 20 8.73068 20 8.32514V1.53212Z" fill-rule="evenodd"/>
                            <path d="M15.9028 8.22958C14.7801 9.36148 12.9597 9.36148 11.837 8.22958C10.7142 7.09769 10.7142 5.26253 11.837 4.13064C12.9597 2.99875 14.7801 2.99875 15.9028 4.13064C17.0256 5.26253 17.0256 7.09769 15.9028 8.22958ZM12.5534 7.50734C13.2805 8.24034 14.4593 8.24034 15.1864 7.50734C15.9135 6.77433 15.9135 5.58589 15.1864 4.85289C14.4593 4.11988 13.2805 4.11988 12.5534 4.85289C11.8263 5.58589 11.8263 6.77433 12.5534 7.50734Z"/>
                          </svg>
                          <div class="rte">{{ block.settings.text }}</div>
                        </div>
                      {%- endif -%}
                      <div class="countdown f-bold"></div>
                    </count-down>
                  </div>
                {%- endif -%}
              {%- when 'description' -%}
                {%- if product.description != blank -%}
                  <div class="product__description rte element-spacing-all{% if settings.animations_reveal_on_scroll %} scroll-trigger animate--fade-in{% endif %}" 
                    {{ block.shopify_attributes }}
                    {% if settings.animations_reveal_on_scroll %}
                      style="--animation-order: {{ forloop.index }};
                        --padding-top: {{ block.settings.padding_top }}px;
                        --padding-bottom: {{ block.settings.padding_bottom }}px;"
                    {% else %}
                      style="--padding-top: {{ block.settings.padding_top }}px;
                        --padding-bottom: {{ block.settings.padding_bottom }}px;"
                    {% endif %}
                  >
                    {%- if block.settings.description_type == 'product_desc' -%}
                      {%- liquid 
                        assign number = block.settings.number
                      -%}
                      {{ product.description | replace: '&nbsp;', ' ' | truncatewords: number }}
                    {%- else -%}
                      {{ block.settings.text }}
                    {%- endif -%}
                  </div>
                {%- endif -%}
              {%- when 'custom_liquid' -%}
                {{ block.settings.custom_liquid }}
              {%- when 'collapsible_tab' -%}
                {%- if block.settings.heading != blank or block.settings.page != blank -%}
                  <div class="product__accordion accordion l-h-1 b-top b-bottom{% if settings.animations_reveal_on_scroll %} scroll-trigger animate--fade-in{% endif %}"
                    {{ block.shopify_attributes }}
                    {% if settings.animations_reveal_on_scroll %}
                      style="--animation-order: {{ forloop.index }};
                        --padding-top: {{ block.settings.padding_top }}px;
                        --padding-bottom: {{ block.settings.padding_bottom }}px;"
                    {% else %}
                      style="--padding-top: {{ block.settings.padding_top }}px;
                        --padding-bottom: {{ block.settings.padding_bottom }}px;
                        --cus-o: 1;"
                    {% endif %}
                  >
                    <details id="Details-{{ block.id }}-{{ section.id }}">
                      <summary class="c-pointer pos-relative element-spacing">
                        <div class="summary__title d-flex f-1">
                          {% render 'icon-accordion', icon: block.settings.icon %}
                          <h2
                            class="{% if block.settings.heading_size == 'h4' %}{{ block.settings.heading_size }}{% else %}{{ block.settings.heading_size | prepend: 'heading-size-' }}{% endif %} accordion__title m-0 d-inline-block break {{ block.settings.heading_font }}"
                            style="{% if block.settings.heading_size != 'h4' %}--cus-size: .4rem;{% endif %}"
                          >
                            {{ block.settings.heading | default: block.settings.page.title }}
                          </h2>
                        </div>
                      </summary>
                      <div class="accordion__content{% if block.settings.content_type != 'description' %} rte{% endif %} break" id="ProductAccordion-{{ block.id }}-{{ section.id }}">
                        {%- if block.settings.content_type == 'description' -%}
                          {{ product.description }}
                        {%- else -%}
                          {{ block.settings.content }}
                          {{ block.settings.page.content }}
                        {%- endif -%}
                      </div>
                    </details>
                  </div>
                {%- endif -%}
              {%- when 'popup' -%}
                {%- if block.settings.text != blank or block.settings.page != blank -%}
                  <side-drawer-opener class="popups-popup-modal__opener product-popup-modal__opener d-inline-block no-js-hidden" data-side-drawer="#PopupModal-{{ block.id }}" {{ block.shopify_attributes }}>
                    <button
                      id="ProductPopup-{{ block.id }}"
                      class="product-popup-modal__button bg-transparent b-none link u-link d-inline-block u-u t-button p-0"
                      type="button"
                      aria-haspopup="dialog"
                    >
                      <span>{{ block.settings.text | default: block.settings.page.title }}</span>
                    </button>
                  </side-drawer-opener>
                  <a href="{{ block.settings.page.url }}" class="product-popup-modal__button link u-link d-inline-block u-u t-button p-0 no-js{% if settings.animations_reveal_on_scroll %} scroll-trigger animate--fade-in{% endif %}"
                    {% if settings.animations_reveal_on_scroll %}
                      style="--animation-order: {{ forloop.index }};"
                    {% endif %}
                  >
                    {{- block.settings.text -}}
                  </a>
                {%- endif -%}
              {%- when 'drawer' -%}
                {%- if block.settings.heading != blank -%}
                  <side-drawer-opener class="d-block product-drawer__opener no-js-hidden" data-side-drawer="#Drawer-{{ block.id }}" {{ block.shopify_attributes }}>
                    <button class="t-button bg-transparent color-foreground b-none c-pointer focus-inset d-flex w-100 a-i-center j-c-between u-none b-bottom left" type="button" aria-haspopup="dialog" aria-label="{{ block.settings.heading | escape }}">
                      {{ block.settings.heading | escape }}
                      {{ icon_caret }}
                    </button>
                  </side-drawer-opener>
                  {%- if block.settings.page != blank and  block.settings.page.url != blank -%}
                    <a href="{{ block.settings.page.url }}" class="product-drawer__link link t-button b-none c-pointer focus-inset d-flex w-100 a-i-center j-c-between left u-none b-bottom no-js{% if settings.animations_reveal_on_scroll %} scroll-trigger animate--fade-in{% endif %}">
                      {{ block.settings.heading | escape }}
                    </a>
                  {%- endif -%}
                {%- endif -%}
              {%- when 'variant_picker' -%}
                <div class="product__variant element-spacing-all{% if settings.animations_reveal_on_scroll %} scroll-trigger animate--fade-in{% endif %}"
                  {% if settings.animations_reveal_on_scroll %}
                    style="--animation-order: {{ forloop.index }};
                      --padding-top: {{ block.settings.padding_top }}px;
                      --padding-bottom: {{ block.settings.padding_bottom }}px;"
                  {% else %}
                    style="--padding-top: {{ block.settings.padding_top }}px;
                      --padding-bottom: {{ block.settings.padding_bottom }}px;"
                  {% endif %}
                >
                  {% render 'product-variant-picker', product: product, block: block, product_form_id: product_form_id %}
                </div>
              {%- when 'buy_buttons' -%}
                <div class="product__action element-spacing-all {{ block.settings.show_quantity_selector }}{% if settings.animations_reveal_on_scroll %} scroll-trigger animate--fade-in{% endif %}"
                  {{ block.shopify_attributes }}
                  {% if settings.animations_reveal_on_scroll %}
                    style="--animation-order: {{ forloop.index }};
                      --padding-top: {{ block.settings.padding_top }}px;
                      --padding-bottom: {{ block.settings.padding_bottom }}px;"
                  {% else %}
                    style="--padding-top: {{ block.settings.padding_top }}px;
                      --padding-bottom: {{ block.settings.padding_bottom }}px;"
                  {% endif %}
                >
                  {%- render 'buy-buttons', 
                    block: block, 
                    product: product, 
                    product_form_id: product_form_id, 
                    section_id: section.id, 
                    show_pickup_availability: block.settings.show_pickup_availability, 
                    show_quantity_selector: block.settings.show_quantity_selector, 
                    show_gift_card_recipient: block.settings.show_gift_card_recipient, 
                    show_wishlist_product: block.settings.show_wishlist_product,
                    type_style: section.settings.type_style
                  -%}
                </div>
              {%- when 'rating' -%}
                <div class="product__rating d-flex a-i-center element-spacing-all{% if settings.animations_reveal_on_scroll %} scroll-trigger animate--fade-in{% endif %}" 
                  {{ block.shopify_attributes }}
                  {% if settings.animations_reveal_on_scroll %}
                    style="--animation-order: {{ forloop.index }};
                      --padding-top: {{ block.settings.padding_top }}px;
                      --padding-bottom: {{ block.settings.padding_bottom }}px;"
                  {% else %}
                    style="--padding-top: {{ block.settings.padding_top }}px;
                      --padding-bottom: {{ block.settings.padding_bottom }}px;"
                  {% endif %}
                >
                  {%- if product.metafields.reviews.rating.value != blank -%}
                    {% liquid
                      assign rating_decimal = 0
                      assign decimal = product.metafields.reviews.rating.value.rating | modulo: 1
                      if decimal >= 0.3 and decimal <= 0.7
                        assign rating_decimal = 0.5
                      elsif decimal > 0.7
                        assign rating_decimal = 1
                      endif
                    %}
                    <div class="rating d-inline-block" role="img" aria-label="{{ 'accessibility.star_reviews_info' | t: rating_value: product.metafields.reviews.rating.value, rating_max: product.metafields.reviews.rating.value.scale_max }}">
                      <span aria-hidden="true" class="rating-star d-inline-block l-h-1" style="--rating: {{ product.metafields.reviews.rating.value.rating | floor }}; --rating-max: {{ product.metafields.reviews.rating.value.scale_max }}; --rating-decimal: {{ rating_decimal }};"
                      ></span>
                    </div>
                    <p class="rating-text caption m-0 d-none">
                      <span aria-hidden="true">
                        {{- product.metafields.reviews.rating.value }} /
                        {{ product.metafields.reviews.rating.value.scale_max -}}
                      </span>
                    </p>
                    <p class="rating-count d-inline-block m-0{% if section.settings.type_style != 'special' %} link{% endif %}">
                      <span aria-hidden="true">
                        {%- if section.settings.type_style == 'special' -%}
                          (
                        {%- endif -%}
                        {{- product.metafields.reviews.rating_count }}
                        {{ product.metafields.reviews.rating_count | pluralize: 'review', 'reviews' -}}
                        {%- if section.settings.type_style == 'special' -%}
                          )
                        {%- endif -%}
                      </span>
                      <span class="visually-hidden">
                        {{- product.metafields.reviews.rating_count }}
                        {{ 'accessibility.total_reviews' | t -}}
                      </span>
                    </p>
                  {%- else -%}
                    <div class="rating d-inline-block" role="img" aria-label="{{ 'accessibility.no_review' | t }}">
                      <span aria-hidden="true" class="rating-star d-inline-block l-h-1" style="--rating: 0; --rating-max: 5; --rating-decimal: 0;"></span>
                    </div>
                    <span class="spr-badge-caption rating-count link d-inline-block m-0">
                      {{- 'accessibility.no_review' | t -}}
                    </span>
                  {%- endif -%}
                  {%- if block.settings.link_to_review -%}
                    <a class="link u-link u-u link--style-line m-l" style="--m-l: 4rem;" href="#shopify-product-reviews">
                      <span class="link-style">
                        Write A Review
                      </span>
                    </a>
                  {%- endif -%}
                </div>
              {%- when 'complementary' -%}
                <product-recommendations class="complementary-products d-block element-spacing-all no-js-hidden{% if block.settings.enable_quick_add %} complementary-products-contains-quick-add{% endif %}{% if settings.animations_reveal_on_scroll %} scroll-trigger animate--fade-in{% endif %}" 
                  data-url="{{ routes.product_recommendations_url }}?section_id={{ section.id }}&product_id={{ product.id }}&limit={{ block.settings.product_list_limit }}&intent=complementary" 
                  {{ block.shopify_attributes }}
                  {% if settings.animations_reveal_on_scroll %}
                    style="--animation-order: {{ forloop.index }};
                      --padding-top: {{ block.settings.padding_top }}px;
                      --padding-bottom: {{ block.settings.padding_bottom }}px;"
                  {% else %}
                    style="--padding-top: {{ block.settings.padding_top }}px;
                      --padding-bottom: {{ block.settings.padding_bottom }}px;"
                  {% endif %}
                >
                  {%- if recommendations.performed and recommendations.products_count > 0 -%}
                    <aside aria-label="{{ 'accessibility.complementary_products' | t }}">
                      <div class="complementary-products__container">
                        <h2 class="h3 complementary-products__title u-none b-bottom">{{ block.settings.heading }}</h2>
                        <slider-component
                          class="complementary-products__slider d-block pos-relative"
                          data-space-between="10"
                          data-type-pagination="dots"
                          data-item-to-show-desktop="1"
                          data-item-to-show-tablet="1"
                          data-item-to-show-mobile="1"
                        >
                          <div class="swiper d-block pos-relative no-js-overflow-x"> 
                            {%- assign number_of_slides = recommendations.products_count | plus: 0.0 | divided_by: block.settings.products_per_page | ceil -%}
                            <div id="Slider-{{ block.id }}" class="swiper-wrapper d-flex f-nowrap"{% if number_of_slides > 1 %} aria-label="{{ 'general.slider.name' | t }}"{% endif %}>
                              {%- for i in (1..number_of_slides) -%}
                                <div id="Slide-{{ block.id }}-{{ forloop.index }}" class="complementary-slide swiper-slide no-js-grid__item no-js-grid__scroll" tabindex="-1" role="group"{% if number_of_slides > 1 %} aria-roledescription="{{ 'sections.slideshow.slide' | t }}" aria-label="{{ forloop.index }} {{ 'general.slider.of' | t }} {{ forloop.length }}"{% endif %}>
                                  <ul class="d-flex f-column list-unstyled grid" role="list">
                                    {%- for product in recommendations.products limit: block.settings.products_per_page offset: continue -%}
                                      {%- liquid
                                        assign ratio = 1
                                        if product.featured_media and block.settings.image_ratio == 'adapt'
                                          assign ratio = product.featured_media.aspect_ratio
                                        endif
                                        if ratio == 0 or ratio == null
                                          assign ratio = 1
                                        endif
                                      -%}
                                      <li>
                                        <div class="card card--horizontal d-flex f-row a-i-start h-100 pos-relative">
                                          <div class="card__inner pos-relative o-h w-100">
                                            <div class="card__media o-h">
                                              <div class="media media--transparent media--{{ block.settings.image_ratio }} pos-relative o-h d-block"
                                                {% if block.settings.image_ratio == 'adapt' %}
                                                  style="padding-bottom: {{ 1 | divided_by: ratio | times: 100 }}%;"
                                                {% endif %}
                                              >
                                                {%- if product.featured_media -%}
                                                  <img
                                                    srcset="{{ product.featured_media | image_url: width: 100 }}"
                                                    src="{{ product.featured_media | image_url: width: 100 }}"
                                                    sizes="{{ product.featured_media.width }}"
                                                    alt="{{ product.featured_media.alt | escape }}"
                                                    class="motion-reduce"
                                                    loading="lazy"
                                                    width="{{ product.featured_media.width }}"
                                                    height="{{ product.featured_media.height }}"
                                                  >
                                                {%- else -%}
                                                  {%- liquid
                                                    assign index = forloop.index
                                                    if index > 6
                                                      assign index = index | modulo: 6 | plus: 1
                                                    endif
                                                  -%}
                                                  {{ 'product-' | append: index | placeholder_svg_tag: 'placeholder-svg' }}
                                                {%- endif -%}
                                              </div>
                                            </div>
                                          </div>
                                          <div class="card__content d-grid w-100">
                                            <div class="card__information">
                                              <h3 class="card__heading o-h" id="title-{{ section_id }}-{{ product.id }}">
                                                <a href="{{ product.url }}" 
                                                  id="CardLink-{{ section_id }}-{{ product.id }}" 
                                                  class="d-block u-none color-current h-link link-cover" 
                                                  aria-labelledby="CardLink-{{ section_id }}-{{ product.id }}"
                                                >
                                                  {{- product.title | escape -}}
                                                </a>
                                              </h3>
                                              <div class="card__price">
                                                {% render 'price',
                                                  product: product,
                                                  price_class: '',
                                                  main_price: true
                                                %}
                                              </div>
                                              {%- if block.settings.show_swatch and product.has_only_default_variant == false -%}
                                                {%- liquid
                                                  assign swatch = settings.swatch | split: "," | strip | downcase | handle
                                                -%}
                                                {%- for option in product.options_with_values -%}
                                                  {%- liquid
                                                    assign is_swatch = false
                                                    assign option_name = option.name | downcase
                                                    if swatch contains option_name
                                                      assign is_swatch = true
                                                      assign option_size = option.values | size
                                                      assign variants_option1_arr = product.variants | map: 'option1'
                                                      assign variants_option2_arr = product.variants | map: 'option2'
                                                      assign variants_option3_arr = product.variants | map: 'option3'
                                                    endif
                                                  -%}
                                                  {%- if is_swatch -%}
                                                    <div class="card__swatch d-flex f-wrap a-i-center">
                                                      {%- for value in option.values -%}
                                                        {% unless forloop.index > 4 %}
                                                          {%- liquid
                                                            assign variant_image = ''
                                                            for variant in product.variants
                                                              case option.position
                                                                when 1
                                                                  if variants_option1_arr[forloop.index0] == value and product.variants[forloop.index0].image
                                                                    assign variant_image = product.variants[forloop.index0].image
                                                                    break
                                                                  endif
                                                                when 2
                                                                  if variants_option2_arr[forloop.index0] == value and product.variants[forloop.index0].image
                                                                    assign variant_image = product.variants[forloop.index0].image
                                                                    break
                                                                  endif
                                                                when 3
                                                                  if variants_option3_arr[forloop.index0] == value and product.variants[forloop.index0].image
                                                                    assign variant_image = product.variants[forloop.index0].image
                                                                    break
                                                                  endif
                                                              endcase
                                                            endfor
                                                          -%}
                                                          <span class="card__swatch--item d-block pos-relative z-index-2 c-pointer{% unless variant_image != blank and image_bg_url != '' %} pattern-color{% endunless %}"
                                                            title="{{ value }}" {% unless variant_image != blank and image_bg_url != '' %} style="--border-variant-color: {{ value | split: ' ' | last | handle }};"{% endunless %}
                                                          >
                                                            {%- if variant_image != blank -%}
                                                              <span class="pattern d-block w-100 h-100 pos-relative center" style="background-image: url({{ variant_image.src | product_img_url: '30x'}});" role="none"></span>
                                                            {%- else -%}
                                                              {%- liquid
                                                                assign image_bg = value | handle | append: '.' | append: 'png'
                                                                assign image_bg_url = ''
                                                                if images[image_bg] != blank
                                                                  assign image_bg_url = image_bg | file_url
                                                                else
                                                                  assign image_bg = value | handle | append: '.' | append: 'jpg'
                                                                  if images[image_bg] != blank
                                                                    assign image_bg_url = image_bg | file_url
                                                                  endif
                                                                endif
                                                              -%}
                                                              {%- if image_bg_url != '' %}
                                                                <span class="pattern d-block w-100 h-100 pos-relative center" style="background-image: url({{ image_bg_url }})" role="none"></span>
                                                              {%- else -%}
                                                                <span class="pattern pattern-color d-block w-100 h-100 pos-relative center{% if value contains 'white' or value contains 'White' %} pattern-white{% endif %} {{ value }}" style="background-color: {{ value | split: ' ' | last | handle }};" role="none"></span>
                                                              {%- endif -%}
                                                            {%- endif -%}
                                                          </span>
                                                        {%- endunless -%}
                                                      {%- endfor -%}
                                                      {%- if option_size > 4 -%}
                                                        {%- assign count = option_size | minus: 4 -%}
                                                        <span class="card__swatch--count color-light">
                                                          {{ 'products.product.more_variants' | t: count: count }}
                                                        </span>
                                                      {%- endif -%}
                                                    </div>
                                                    {% break %}
                                                  {%- endif -%}
                                                {%- endfor -%}
                                              {%- endif -%}
                                              <div class="card__link link link--text u-u">
                                                {{ 'products.product.details' | t }}
                                              </div>
                                            </div>
                                          </div>
                                        </div>
                                      </li>
                                    {%- endfor -%}
                                  </ul>
                                </div>
                              {%- endfor -%}
                            </div>
                            {%- if number_of_slides > 1 -%}
                              <div class="slideshow__controls pos-relative pos-relative-xs">
                                <div class="swiper-pagination pos-relative"></div>
                              </div>
                            {%- endif -%}
                          </div>
                        </slider-component>
                      </div>
                    </aside>
                  {%- endif -%}
                </product-recommendations>
              {%- when 'icon-with-text' -%}
                <div class="product__icons element-spacing-all{% if settings.animations_reveal_on_scroll %} scroll-trigger animate--fade-in{% endif %}" 
                  {{ block.shopify_attributes }}
                  {% if settings.animations_reveal_on_scroll %}
                    style="--animation-order: {{ forloop.index }};
                      --padding-top: {{ block.settings.padding_top }}px;
                      --padding-bottom: {{ block.settings.padding_bottom }}px;"
                  {% else %}
                    style="--padding-top: {{ block.settings.padding_top }}px;
                      --padding-bottom: {{ block.settings.padding_bottom }}px;"
                  {% endif %}
                >
                  <ul class="icon-with-text icon-with-text--{{ block.settings.layout }}{% if block.settings.layout == 'horizontal' %} d-flex f-row j-c-center{% endif %} list-unstyled">
                    {%- if block.settings.heading_1 != blank -%}
                      <li class="icon-with-text__item d-flex a-i-center{% if block.settings.layout == 'horizontal' %} f-column{% endif %}">
                        {%- if block.settings.image_1 == null -%}
                          {%- render 'icon-accordion', icon: block.settings.icon_1 -%}
                        {%- else -%}
                          <img
                            src="{{ block.settings.image_1 | image_url }}"
                            {% if block.settings.image_1.alt != blank %}
                              alt="{{ block.settings.image_1.alt | escape }}"
                            {% else %}
                              role="presentation"
                            {% endif %}
                            height="auto"
                            width="auto"
                            loading="lazy"
                          >
                        {%- endif -%}
                        <span class="h4 m-0{% if block.settings.layout == 'horizontal' %} center{% endif %}">
                          {{- block.settings.heading_1 -}}
                        </span>
                      </li>
                    {%- endif -%}
                    {%- if block.settings.heading_2 != blank -%}
                      <li class="icon-with-text__item d-flex a-i-center{% if block.settings.layout == 'horizontal' %} f-column{% endif %}">
                        {%- if block.settings.image_2 == null -%}
                          {% render 'icon-accordion', icon: block.settings.icon_2 -%}
                        {%- else -%}
                          <img
                            src="{{ block.settings.image_2 | image_url }}"
                            {% if block.settings.image_2.alt != blank %}
                              alt="{{ block.settings.image_2.alt | escape }}"
                            {% else %}
                              role="presentation"
                            {% endif %}
                            height="auto"
                            width="auto"
                            loading="lazy"
                          >
                        {%- endif -%}
                        <span class="h4 m-0{% if block.settings.layout == 'horizontal' %} f-column{% endif %}">
                          {{- block.settings.heading_2 -}}
                        </span>
                      </li>
                    {%- endif -%}
                    {%- if block.settings.heading_3 != blank -%}
                      <li class="icon-with-text__item d-flex a-i-center{% if block.settings.layout == 'horizontal' %} f-column{% endif %}">
                        {%- if block.settings.image_3 == null -%}
                          {% render 'icon-accordion', icon: block.settings.icon_3 -%}
                        {%- else -%}
                          <img
                            src="{{ block.settings.image_3 | image_url }}"
                            {% if block.settings.image_3.alt != blank %}
                              alt="{{ block.settings.image_3.alt | escape }}"
                            {% else %}
                              role="presentation"
                            {% endif %}
                            height="auto"
                            width="auto"
                            loading="lazy"
                          >
                        {%- endif -%}
                        <span class="h4 m-0{% if block.settings.layout == 'horizontal' %} f-column{% endif %}">
                          {{- block.settings.heading_3 -}}
                        </span>
                      </li>
                    {%- endif -%}
                  </ul>
                </div>
              {%- when 'trust_badge' -%}
                {%- if block.settings.image != blank -%}
                  <div class="product__trust-badge element-spacing-all{% if settings.animations_reveal_on_scroll %} scroll-trigger animate--fade-in{% endif %}" 
                    {{ block.shopify_attributes }}
                    {% if settings.animations_reveal_on_scroll %}
                      style="--animation-order: {{ forloop.index }};
                        --padding-top: {{ block.settings.padding_top }}px;
                        --padding-bottom: {{ block.settings.padding_bottom }}px;"
                    {% else %}
                      style="--padding-top: {{ block.settings.padding_top }}px;
                        --padding-bottom: {{ block.settings.padding_bottom }}px;"
                    {% endif %}
                  >
                    {%- if block.settings.text != blank -%}
                      <div class="product__trust-badge--text left caption-large">
                        {{ block.settings.text }}
                      </div>
                    {%- endif -%}
                    <div class="product__trust-badge--image">
                      {%- liquid
                        assign image_height = block.settings.image.width | divided_by: block.settings.image.aspect_ratio | round
                      -%}
                      {{ block.settings.image | image_url: width: 750 | image_tag:
                        loading: 'lazy',
                        width: block.settings.image.width,
                        height: image_height,
                        class: '',
                        sizes: '100vw',
                        widths: '375, 550, 750',
                        alt: block.settings.image.alt | escape
                      }}
                    </div>
                  </div>
                {%- endif -%}
              {%- when 'positive_vibes_bundle' -%}
                <slider-component class="product__positive-bundle carousel vertical d-block pos-relative element-spacing-all" 
                  {{ block.shopify_attributes }}
                  data-slider-autoplay="true"
                  data-space-between-desktop="0"
                  data-space-between-tablet="0"
                  data-space-between="0"
                  data-type-pagination="none"
                  data-item-to-show-desktop="1"
                  data-item-to-show-tablet="1"
                  data-item-to-show-mobile="1"
                  data-loop="true"
                  data-mousewheel="false"
                  data-direction-desktop="vertical"
                  data-direction-mobile="vertical"
                  style="
                    --padding-top: {{ block.settings.padding_top }}px;
                    --padding-bottom: {{ block.settings.padding_bottom }}px;
                  "
