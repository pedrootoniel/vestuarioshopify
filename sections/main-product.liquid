                    {%- endif -%}
                    {%- if block.settings.ask_an_expert_option_2_1 != blank -%}
                      <input
                        class="form-radio"
                        checked
                        id="askAnExpertRadio21"
                        type="radio"
                        name="contact[{{ block.settings.ask_an_expert_option_2 | escape | default: 'Option 2'  }}]"
                        value="{{ block.settings.ask_an_expert_option_2_1 | split: '|' | first }}"
                      >
                      <label
                        class="form-label form-label--radio c-pointer pos-relative d-inline-block"
                        for="askAnExpertRadio21"
                      >
                        {{- block.settings.ask_an_expert_option_2_1 | escape -}}
                      </label>
                    {%- endif -%}
                    {%- if block.settings.ask_an_expert_option_2_2 != blank -%}
                      <input
                        class="form-radio"
                        id="askAnExpertRadio22"
                        type="radio"
                        name="contact[{{ block.settings.ask_an_expert_option_2 | escape | default: 'Option 2'  }}]"
                        value="{{ block.settings.ask_an_expert_option_2_2 | split: '|' | first }}"
                      >
                      <label
