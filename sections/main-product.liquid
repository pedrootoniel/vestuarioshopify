                          {{ 'products.product.size_chart.name' | t }}
                        </span>
                      </button>
                    </side-drawer-opener>
                  </div>
                {%- endif -%}
            {%- endcase -%}
          {%- endfor -%}
        </product-info>
        {%- if section.settings.enable_sticky_add_to_cart -%}
          {% render 'sticky-add-to-cart', product: product, product_form_id: product_form_id %}
        {%- endif -%}
      </div>
    </div>
  </div>
