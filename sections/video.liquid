<slideshow-component class="section__v-banner d-block spacing spacing-xs color-{{ section.settings.color_scheme }} gradient"
  style="--padding-top: {{ section.settings.padding_top }}px;
    --padding-bottom: {{ section.settings.padding_bottom }}px;
    --padding-top-xs: {{ section.settings.padding_top_mobile }}px;
    --padding-bottom-xs: {{ section.settings.padding_bottom_mobile }}px;
  {% if section.settings.mobile_color_scheme == 'custom' %}
    --color-foreground-xs: {{ section.settings.colors_text.red }}, {{ section.settings.colors_text.green }}, {{ section.settings.colors_text.blue }};
    --color-background-xs: {{ section.settings.colors_background.red }}, {{ section.settings.colors_background.green }}, {{ section.settings.colors_background.blue }};
    --gradient-background-xs: {% if section.settings.gradient_background != blank %}{{ section.settings.gradient_background }}{% else %}{{ section.settings.colors_background }}{% endif %};
  {% endif %}" 
>
  {%- liquid
    unless section.settings.enable_autoplay
      capture icon_play
        render 'icon-play'
      endcapture
    endunless
  -%}
  <div class="page-width m-auto">
    <div class="banner d-flex f-wrap pos-relative i-padding i-padding-md i-padding-lg{% if section.settings.cover_mobile_image != blank %} b-image-mobile{% endif %}{% if section.settings.use_custom_colors_background %} custom-bg--video bg-custom{% endif %}{% if section.settings.mobile_color_scheme == 'custom' %} gradient gradient-xs{% endif %} banner__media-hover"
      id="Video-{{ section.id }}"
      style="--i-padding: {% if section.settings.cover_image != blank %}{{ 1 | divided_by: section.settings.cover_image.aspect_ratio | times: 100 }}{% else %}56{% endif %}%;
        --media-opacity: {{ section.settings.image_overlay_opacity | divided_by: 100.0 }};
        --color-opacity: {{ section.settings.color_overlay_opacity.red }},{{ section.settings.color_overlay_opacity.green }},{{ section.settings.color_overlay_opacity.blue }};
        {%- if section.settings.use_custom_colors_background %}
          --bg-custom:{{ section.settings.custom_colors_background.rgb }};
        {% endif -%}
        {% if section.settings.cover_mobile_image != blank %}--p-image-mobile: {{ 1 | divided_by: section.settings.cover_mobile_image.aspect_ratio | times: 100 }}%;{% endif %}
      "
    >
      <div class="banner__media media media--transparent d-block o-h w-100 h-100 pos-absolute{% if section.settings.use_custom_colors_background %} pos-absolute-c-c{% else %} top-0 left-0{% endif %}{% if section.blocks.size > 0 %} pos-relative-xs{% endif %} rounded">
        <video-template class="d-block w-100 h-100" data-video-type="{{ section.settings.video_url.type }}" data-video-id="{{ section.settings.video_url.id }}" data-autoplay="{{ section.settings.enable_autoplay }}" id="VIDEO-{{ section.id }}">
          {%- liquid
              assign video_id = section.settings.video.id | default: section.settings.video_url.id
              assign video_alt = section.settings.video.alt | default: section.settings.description
              assign poster = section.settings.cover_image | default: section.settings.video.preview_image
              assign autoplay = ''
              assign loop = ''
              if section.settings.enable_autoplay
                  assign autoplay = '1'
              endif
              if section.settings.enable_video_looping
                  assign loop = '&loop=1&playlist=' | append: video_id
              endif
              if section.settings.cover_mobile_image != blank
                assign class = 'd-none d-block-md w-100 h-100 o-f-cover motion-reduce'
              else
                assign class = 'w-100 h-100 o-f-cover motion-reduce'
              endif
          -%}
          {%- if section.settings.cover_image != blank -%}
            {%- capture sizes -%}
              (min-width: {{ settings.page_width }}px) {{ settings.page_width | minus: 100 }}px, (min-width: 750px)
                calc(100vw - 10rem), 100vw
            {%- endcapture -%}
            {%- assign alt = 'sections.video.load_video' | t: description: section.settings.description | escape -%}
            {{ section.settings.cover_image | image_url: width: 3840 | image_tag: 
              loading: 'lazy', 
              sizes: sizes, 
              widths: '375, 750, 1100, 1500, 1780, 2000, 3000, 3840', 
              alt: alt,
              class: class
            }}
          {%- else -%}
            {{ 'lifestyle-1' | placeholder_svg_tag: 'placeholder-svg placeholder pos-absolute top-0 left-0 w-100 h-100' }}
          {%- endif -%}

          {%- if section.settings.cover_mobile_image != blank -%}
            {%- assign alt = 'sections.video.load_video' | t: description: section.settings.description | escape -%}
            {{ section.settings.cover_mobile_image | image_url: width: section.settings.cover_mobile_image.width | image_tag:
              loading: 'lazy',
              sizes: '(min-width: 361px) calc(100vw/2), calc(100vw/3)',
              widths: '750',
              alt: alt,
              class: 'd-block d-none-md w-100 h-100 o-f-cover motion-reduce'
            }}
          {%- endif -%}

          {%- if section.settings.video == null and section.settings.video_url != null -%}
            <iframe
                tabindex="-1"
                {%- if section.settings.video_url.type == 'youtube' -%}
                  data-src="https://www.youtube.com/embed/{{ video_id }}?enablejsapi=1&mute=1&autoplay={{ autoplay }}{{ loop }}" 
                {%- else -%}
                  src="https://player.vimeo.com/video/{{ video_id }}?autoplay={{ autoplay }}{{ loop }}
                {%- endif -%}"    
                class="d-block pos-absolute pos-absolute-c-c w-100 h-100 b-none no-js-hidden {% if section.settings.video_url.type == 'youtube' %}js-youtube{%- else -%}js-video{%- endif -%}"
                allow="autoplay; encrypted-media"
                allowfullscreen
                title="{{ section.settings.video_description | default: shop.name | escape }}"
            ></iframe>
          {%- else -%}
            {{
              section.settings.video
              | video_tag:
                image_size: '1100x',
                autoplay: section.settings.enable_autoplay,
                loop: section.settings.enable_video_looping,
                controls: true,
                muted: false,
                class: 'pos-absolute top-0 left-0 w-100 h-100'
            }}
          {%- endif -%}

        </video-template>
        {%- unless section.settings.enable_autoplay -%}
          <video-template-opener data-video="#VIDEO-{{ section.id }}" class="deferred-media__poster-button pos-absolute pos-absolute-c-c z-index-1 b-circle c-pointer button-play--{{ section.settings.video_play_type }}{% if section.settings.video_play_type == 'icon' %} gradient gradient-global{% endif %} motion-reduce d-flex a-i-center j-c-center{% if section.blocks.size > 0 %} d-none-md{% endif %}">
            {%- if section.settings.video_play_type == 'icon' -%}
              <button class="t-button b-none c-pointer bg-transparent d-flex a-i-center p-0 color-current" type="button" aria-label="{{ 'sections.video.load_video' | t: description: video_description | escape }}">
                {{ icon_play }}
              </button>
            {%- else -%}
              <button class="t-button b-none c-pointer bg-transparent d-flex a-i-center p-0 color-current button button--circle b-transparent" type="button" aria-label="{{ 'sections.video.load_video' | t: description: video_description | escape }}"
                style="--circle-diameter: 164px;"
              >
                <span class="text uppercase">{{ 'sections.video.play_video' | t }}</span>
                <span class="button--icon-circle">
                  {{ icon_play }}
                </span>
              </button>
            {%- endif -%}
          </video-template-opener>
        {%- endunless-%}
      </div>
      {%- if section.blocks.size > 0 -%}
        <div class="banner__content banner__content--{{ section.settings.desktop_content_position }} d-flex a-i-center j-c-center f-shrink f-grow pos-relative w-100 z-index-2 o-h">
          <animate-element class="banner__box d-flex f-column j-c-center w-100 h-100 {{ section.settings.desktop_content_alignment }} {{ section.settings.mobile_content_alignment }}-xs">
            {%- for block in section.blocks -%}
              {%- case block.type -%}
                {%- when 'heading' -%}
                  {%- if block.settings.heading != blank -%}
                    <h2 class="banner__heading {{ block.settings.heading_size | prepend: 'heading-size-' }} element-spacing element-spacing-all data__media-hover" data-animate="{{ section.settings.text_animation }}" {{ block.shopify_attributes }}
                      style="--padding-top: {{ block.settings.padding_top }}px;
                        --padding-bottom: {{ block.settings.padding_bottom }}px;
                        --a-delay: {{ '0.2' | times: forloop.index }}s;"
                    >{{ block.settings.heading | escape }}</h2>
                  {%- endif -%}
                {%- when 'text' -%}
                  {%- if block.settings.text != blank -%}
                    <div class="banner__text rte element-spacing element-spacing-all data__media-hover" data-animate="{{ section.settings.text_animation }}" {{ block.shopify_attributes }}
                      style="--padding-top: {{ block.settings.padding_top }}px;
                        --padding-bottom: {{ block.settings.padding_bottom }}px;
                        --a-delay: {{ '0.2' | times: forloop.index }}s;"
                    >{{ block.settings.text }}</div>
                  {%- endif -%}
                {%- when 'countdown' -%}
                  {%- if block.settings.time != blank -%}
                    <div class="banner__countdown hidden data__media-hover"
                      style="--color-foreground: {{ block.settings.colors_text.red }}, {{ block.settings.colors_text.green }}, {{ block.settings.colors_text.blue }};
                        --color-background: {{ block.settings.colors_background.red }}, {{ block.settings.colors_background.green }}, {{ block.settings.colors_background.blue }};
                        --gradient-background: {% if block.settings.gradient_background != blank %}{{ block.settings.gradient_background }}{% else %}{{ block.settings.colors_background }}{% endif %};
                        --padding-top: {{ block.settings.padding_top }}px;
                        --padding-bottom: {{ block.settings.padding_bottom }}px;
                        --a-delay: {{ '0.2' | times: forloop.index }}s;"
                      data-animate="{{ section.settings.text_animation }}" {{ block.shopify_attributes }}
                    >
                      <count-down class="d-block element-spacing element-spacing-all" data-countdown="{{ block.settings.time }}" data-type="banner">
                        <div class="countdown d-flex a-i-center j-c-center gradient pos-relative o-h m-auto"></div>
                      </count-down>
                    </div>
                  {%- endif -%}
                {%- when 'buttons' -%}
                  {%- if block.settings.primary_button_label != blank or block.settings.secondary_button_label != blank -%}
                    <div class="banner__buttons break d-flex f-wrap g-gap element-spacing element-spacing-all data__media-hover j-c-{{ section.settings.desktop_content_alignment | replace: 'left', 'start' | replace: 'right', 'end' }} j-c-{{ section.settings.mobile_content_alignment | replace: 'left', 'start' | replace: 'right', 'end' }}-xs" data-animate="{{ section.settings.text_animation }}" {{ block.shopify_attributes }}
                      style="--padding-top: {{ block.settings.padding_top }}px;
                        --padding-bottom: {{ block.settings.padding_bottom }}px;
                        --g-gap: 1rem;
                        --a-delay: {{ '0.2' | times: forloop.index }}s;"
                    >
                      {%- if block.settings.primary_button_label != blank -%}
                        <a {% if block.settings.primary_button_link == blank %}role="link" aria-disabled="true"{% else %}href="{{ block.settings.primary_button_link }}"{% endif %} class="button {{ settings.button_type | prepend: 'button--main-' }} {{ settings.button_icon | prepend: 'button--icon-' }} {{ settings.button_style | prepend: 'button--style-' }}  button--transparent">
                          <span class="text">{{- block.settings.primary_button_label | escape -}}</span>
                          {% render 'button-main-style' %}
                        </a>
                      {%- endif -%}
                      {%- if block.settings.secondary_button_label != blank -%}
                        <a {% if block.settings.secondary_button_link == blank %}role="link" aria-disabled="true"{% else %}href="{{ block.settings.secondary_button_link }}"{% endif %} class="button {{ settings.button_type | prepend: 'button--main-' }} {{ settings.button_icon | prepend: 'button--icon-' }} {{ settings.button_style | prepend: 'button--style-' }}  button--transparent">
                          <span class="text">{{- block.settings.secondary_button_label | escape -}}</span>
                          {% render 'button-main-style' %}
                        </a>
                      {%- endif -%}
                    </div>
                  {%- endif -%}
              {%- endcase -%}
            {%- endfor -%}
            {%- unless section.settings.enable_autoplay -%}
              <video-template-opener data-video="#VIDEO-{{ section.id }}" class="video__opener d-none-xs d-block data__media-hover d-flex j-c-{{ section.settings.desktop_content_alignment | replace: 'left', 'start' | replace: 'right', 'end' }} j-c-{{ section.settings.mobile_content_alignment | replace: 'left', 'start' | replace: 'right', 'end' }}-xs" data-animate="{{ section.settings.text_animation }}" style="--a-delay: {{ '0.2' | times: section.blocks.size }}s;">
                <button class="t-button b-none c-pointer bg-transparent color-current g-gap d-flex a-i-center p-0" type="button" aria-label="{{ 'sections.video.load_video' | t: description: video_description | escape }}" style="--g-gap: 1rem;">
                  {{ icon_play }}
                  <span class="text">{{ 'sections.video.play' | t }}</span>
                </button>
              </video-template-opener>
            {%- endunless-%}
          </animate-element>
        </div>
      {%- endif -%}
    </div>
    {%- if section.settings.below_text != blank and section.settings.below_link_text != blank -%}
      <div class="d-flex f-wrap a-i-center b-bottom g-gap rte gradient" 
        style="
          padding-top: 2.4rem; padding-bottom: 2.4rem; --g-gap: 0.5rem;
          --color-foreground: {{ section.settings.colors_below_text.red }}, {{ section.settings.colors_below_text.green }}, {{ section.settings.colors_below_text.blue }};
        "
      >
        {%- if section.settings.below_text != blank -%}
          <div>{{ section.settings.below_text }}</div>
        {%- endif -%}
        {%- if section.settings.below_link_text != blank -%}
          <a {% if section.settings.below_link == blank %} role="link" aria-disabled="true"{% else %} href="{{ section.settings.below_link }}"{% endif %} class="link u-u h-link link--style-line link-arrow d-flex a-i-center animate-arrow f-bold" style="text-decoration-thickness: 0.2rem; --color-link: var(--color-sale-price);">
            <span class="text link-style">{{ section.settings.below_link_text | escape }}</span>
            {% render 'icon-arrow' %}
          </a>
        {%- endif -%}
      </div>
    {%- endif -%}
  </div>
</slideshow-component>
{% schema %}
{
  "name": "t:sections.video.name",
  "tag": "section",
  "class": "section",
  "disabled_on": {
    "groups": ["header", "footer", "custom.popup"]
  },
  "settings": [
  {
    "type": "image_picker",
    "id": "cover_image",
    "label": "t:sections.video.settings.cover_image.label"
  },
  {
    "type": "image_picker",
    "id": "cover_mobile_image",
    "label": "t:sections.video.settings.cover_image.label__1"
  },
  {
    "type": "checkbox",
    "id": "use_custom_colors_background",
    "default": false,
    "label": "t:sections.all.colors.use_custom_color_background"
  },
  {
    "type": "color",
    "id": "custom_colors_background",
    "default": "#021F3C",
    "label": "t:sections.all.colors.set_colors_background_video"
  },
  {
    "type": "header",
    "content": "t:sections.collection-video.blocks.settings.header__1"
  },
  {
    "type": "video",
    "id": "video",
    "label": "t:sections.collection-video.blocks.settings.label__1"
  },
  {
    "type": "video_url",
    "id": "video_url",
    "accept": ["youtube", "vimeo"],
    "default": "https://www.youtube.com/watch?v=_9VUPq3SxOc",
    "label": "t:sections.video.settings.video_url.label",
    "placeholder": "t:sections.video.settings.video_url.placeholder",
    "info": "t:sections.video.settings.video_url.info"
  },
  {
    "type": "text",
    "id": "description",
    "label": "t:sections.video.settings.description.label",
    "info": "t:sections.video.settings.description.info"
  },
  {
    "type": "checkbox",
    "id": "enable_autoplay",
    "default": true,
    "label": "t:sections.video.settings.enable_autoplay.label"
  },
  {
    "type": "checkbox",
    "id": "enable_video_looping",
    "label": "t:sections.video.settings.enable_video_looping.label",
    "default": false
  },
  {
    "type": "select",
    "id": "video_play_type",
    "options": [
    {
      "value": "icon",
      "label": "t:sections.video.settings.enable_autoplay.options__1"
    },
    {
      "value": "text",
      "label": "t:sections.video.settings.enable_autoplay.options__2"
    }],
    "default": "icon",
    "label": "t:sections.video.settings.enable_autoplay.label__1",
    "info": "t:sections.video.settings.enable_autoplay.info"
  },
  {
    "type": "select",
    "id": "desktop_content_position",
    "options": [
    {
      "value": "top-left",
      "label": "t:sections.all.desktop_content_position.options__1"
    },
    {
      "value": "top-center",
      "label": "t:sections.all.desktop_content_position.options__2"
    },
    {
      "value": "top-right",
      "label": "t:sections.all.desktop_content_position.options__3"
    },
    {
      "value": "middle-left",
      "label": "t:sections.all.desktop_content_position.options__4"
    },
    {
      "value": "middle-center",
      "label": "t:sections.all.desktop_content_position.options__5"
    },
    {
      "value": "middle-right",
      "label": "t:sections.all.desktop_content_position.options__6"
    },
    {
      "value": "bottom-left",
      "label": "t:sections.all.desktop_content_position.options__7"
    },
    {
      "value": "bottom-center",
      "label": "t:sections.all.desktop_content_position.options__8"
    },
    {
      "value": "bottom-right",
      "label": "t:sections.all.desktop_content_position.options__9"
    }],
    "default": "middle-center",
    "label": "t:sections.all.desktop_content_position.label",
    "info": "t:sections.all.desktop_content_position.info"
  },
  {
    "type": "select",
    "id": "desktop_content_alignment",
    "options": [
    {
      "value": "left",
      "label": "t:sections.all.desktop_content_alignment.options__1"
    },
    {
      "value": "center",
      "label": "t:sections.all.desktop_content_alignment.options__2"
    },
    {
      "value": "right",
      "label": "t:sections.all.desktop_content_alignment.options__3"
    }],
    "default": "center",
    "label": "t:sections.all.desktop_content_alignment.label"
  },
  {
    "type": "select",
    "id": "mobile_content_alignment",
    "options": [
    {
      "value": "left",
      "label": "t:sections.all.mobile_content_alignment.options__1"
    },
    {
      "value": "center",
      "label": "t:sections.all.mobile_content_alignment.options__2"
    },
    {
      "value": "right",
      "label": "t:sections.all.mobile_content_alignment.options__3"
    }],
    "default": "center",
    "label": "t:sections.all.mobile_content_alignment.label"
  },
  {
    "type": "select",
    "id": "mobile_color_scheme",
    "default": "custom",
    "label": "t:sections.all.mobile_color_scheme.label",
    "info": "t:sections.all.mobile_color_scheme.info",
    "options": [
    {
      "value": "default",
      "label": "t:sections.all.mobile_color_scheme.options__1"
    },
    {
      "value": "custom",
      "label": "t:sections.all.mobile_color_scheme.options__2"
    }]
  },
  {
    "type": "color",
    "id": "colors_text",
    "default": "#424242",
    "label": "t:sections.all.colors.colors_text"
  },
  {
    "type": "color",
    "id": "colors_background",
    "label": "t:sections.all.colors.colors_background"
  },
  {
    "type": "color_background",
    "id": "gradient_background",
    "default": "linear-gradient(90deg, rgba(246, 238, 227, 1), rgba(223, 244, 247, 1) 100%)",
    "label": "t:sections.all.colors.gradient_background"
  },
  {
    "type": "color_scheme",
    "id": "color_scheme",
    "label": "t:sections.all.colors.label",
    "default": "background-1"
  },
  {
    "type": "range",
    "id": "image_overlay_opacity",
    "min": 0,
    "max": 100,
    "step": 10,
    "unit": "%",
    "label": "t:sections.all.image_overlay_opacity.label",
    "default": 30
  },
  {
    "type": "color",
    "id": "color_overlay_opacity",
    "label": "t:sections.all.colors.colors_background",
    "default": "#000000"
  },
  {
    "type": "header",
    "content": "t:sections.video.settings.header__3.content"
  },
  {
    "type": "richtext",
    "id": "below_text",
    "label": "t:sections.all.text.label",
    "default": "<p>End of Season. </p>"
  },
  {
    "type": "color",
    "id": "colors_below_text",
    "default": "#000000",
    "label": "t:sections.all.colors.colors_text"
  },
  {
    "type": "text",
    "id": "below_link_text",
    "label": "t:sections.all.link_label.label",
    "default": "Save 50% off Decorative"
  },
  {
    "type": "url",
    "id": "below_link",
    "label": "t:sections.all.link_url.label"
  },
  {
    "type": "header",
    "content": "t:sections.all.animation.content"
  },
  {
    "type": "select",
    "id": "text_animation",
    "options": [
      {
        "value": "none",
        "label": "t:sections.all.animation.set_animation.options__1.label"
      },
      {
        "value": "fade-up",
        "label": "t:sections.all.animation.set_animation.options__2.label"
      },
      {
        "value": "fade-down",
        "label": "t:sections.all.animation.set_animation.options__3.label"
      },
      {
        "value": "zoom-in",
        "label": "t:sections.all.animation.set_animation.options__4.label"
      },
      {
        "value": "zoom-out",
        "label": "t:sections.all.animation.set_animation.options__5.label"
      }
    ],
    "default": "none",
    "label": "t:sections.all.animation.set_animation.label__2"
  },
  {
    "type": "header",
    "content": "t:sections.all.padding.section_padding_heading"
  },
  {
    "type": "range",
    "id": "padding_top",
    "min": 0,
    "max": 100,
    "step": 1,
    "unit": "px",
    "label": "t:sections.all.padding.padding_top",
    "default": 0
  },
  {
    "type": "range",
    "id": "padding_bottom",
    "min": 0,
    "max": 100,
    "step": 1,
    "unit": "px",
    "label": "t:sections.all.padding.padding_bottom",
    "default": 0
  },
  {
    "type": "range",
    "id": "padding_top_mobile",
    "min": 0,
    "max": 100,
    "step": 1,
    "unit": "px",
    "label": "t:sections.all.padding.padding_top_mobile",
    "default": 0
  },
  {
    "type": "range",
    "id": "padding_bottom_mobile",
    "min": 0,
    "max": 100,
    "step": 1,
    "unit": "px",
    "label": "t:sections.all.padding.padding_bottom_mobile",
    "default": 0
  }],
  "blocks": [
  {
    "type": "heading",
    "name": "t:sections.image-banner.blocks.heading.name",
    "limit": 1,
    "settings": [
    {
      "type": "text",
      "id": "heading",
      "default": "Image banner",
      "label": "t:sections.all.heading.label"
    },
    {
      "type": "select",
      "id": "heading_size",
      "options": [
      {
        "value": "default",
        "label": "t:sections.all.font_size.options"
      },
